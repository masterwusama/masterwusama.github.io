[{"ID": "85", "NAME": "迦尔纳", "NAME_JP": "カルナ", "NAME_EN": "Karna", "PIC1": "085A.png", "PIC2": "085B.png", "PIC3": "085C.png", "PIC4": "085D.png", "PIC5": "085Z.png", "Avatar": "085", "STAR": "5", "CLASS": "Lancer", "LV1_HP": "1999", "LV1_ATK": "1850", "LVMAX0_HP": "7885", "LVMAX0_ATK": "6973", "LVMAX1_HP": "8560", "LVMAX1_ATK": "7561", "LVMAX2_HP": "9851", "LVMAX2_ATK": "8685", "LVMAX3_HP": "11619", "LVMAX3_ATK": "10224", "LVMAX4_HP": "13632", "LVMAX4_ATK": "11976", "LV90_HP": "13632", "LV90_ATK": "11976", "LV100_HP": "14934", "LV100_ATK": "13110", "COST": "12", "ILLUST": "pako", "CV": "游佐浩二", "GLUTEN": "B", "DURABLE": "C", "AGILE": "A", "MAGIC": "B", "LUCKY": "D", "TREASURE": "EX", "SKILL_R1": "贫者之见识", "SKILL_P1": "512.png", "SKILL_L1": "A", "SKILL_C1": "8", "SKILL_E1": ["敌方单体弱体耐性下降（1回合）", "赋予宝具封印状态（1回合）"], "SKILL_R2": "魔力放出（炎）", "SKILL_P2": "306.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["自身的Buster卡性能提升（1回合）", "宝具威力提升（1回合）"], "SKILL_R3": "无冠之武艺", "SKILL_P3": "601.png", "SKILL_C3": "8", "SKILL_L3": "A", "SKILL_E3": ["自身的NP增加", "星星发生率提升（3回合）", "暴击威力提升（3回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "C", "CSKILL_E1": "自身的弱体耐性少许提升（15%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "A", "CSKILL_E2": "自身的Quick卡性能提升（10%)", "CSKILL_R3": "神性", "CSKILL_P3": "326.png", "CSKILL_L3": "A", "CSKILL_E3": "赋予自身伤害提升状态（200)", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "日轮呀，顺从死亡（Vasavi Shakti）", "T_PROP": "Buster", "T_LEVEL": "EX", "T_TYPE": "对神宝具", "T_EFFECT": "对敌方全体进行强力的[神性]特攻攻击<超蓄力威力提升>", "CARD_BUSTER": "2", "CARD_ARTS": "1", "CARD_QUICK": "2", "INTRO1": "印度古代叙事诗《摩诃婆罗多》的大英雄。既是摩诃婆罗多中心的英雄阿周那的对手，也是其异父兄弟。\n由于数多的诅咒缠身而无法自由行动。\n他，被阿周那以近乎谋杀的形式杀死了。", "INTRO2": "身高/体重：178cm・65kg\n出典：摩诃婆罗多\n地域：印度\n属性：秩序・善\u3000\u3000\u3000性別：男性\n也符合Archer、Rider等职阶。", "INTRO3": "迦尔纳是人类女性贡蒂与太阳神苏利耶所生的半神英雄。\n然而，他出生后就立刻被贡蒂抛弃，作为车夫的儿子被抚养长大。\n但是，其作为英雄的资质却从未被掩盖。", "INTRO4": "母亲贡蒂舍弃迦尔纳之后，又生下了般度五兄弟。\n其中的三男，也就是对迦尔纳来说可谓是终生的对手的阿周那。\n成长起来的迦尔纳成为了与般度家对立的俱卢家的养子。", "INTRO5": "然而，和阿周那对战之前，迦尔纳的已是诅咒缠身、障碍累累。\n婆罗门的诅咒，因陀罗的欺骗，连母亲向他的求诉，要他发誓不跟阿周那之外的人战斗，他也一并接受了下来。", "INTRO6": "说到将铠甲转让给因陀罗的逸话，则是迦尔纳把黄金铠甲分解、破坏，而获得了弑神之枪。\n其为众神之王也掌控不了的光枪。\n染遍全身剥离的鲜血而得以放出的一击，任何污秽都能一扫而净。\n\n——日轮啊，顺从死亡。", "INTRO7": "迦尔纳是极为宽大的Servant，一切都能以「事该如此」的理由接受下来。\n他不仅对万般人等平等对待，更是将万般人等当作“各自的花朵”加以尊重。\n拥有绝不会被众多的偏见公正对待的武艺和高洁的精神，迦尔纳即便论其英雄之格也仍能在众Servant中夺得一二。\n\n……虽说是不可能的事情，若有和他契约却不得胜利的Master，那绝对是不肯战斗，一等一的麻烦人物吧。", "Property": "骑乘,人型,神性,从者（被EA克制）,天/地从者,所爱之人", "Camp": "天", "Crit": "12.2%", "Death": "28.0%", "CritPr": "88", "InitiativeNp": "0.72%", "Passive": "4.0%", "ArtHit": "3", "BusHit": "1", "QuiHit": "3", "EXHit": "4", "Height": "178", "Weight": "65", "Origin": "摩诃婆罗多", "Region": "印度", "Attributes": "秩序・善", "Gender": "男性", "T_Text_Arr": "（宝具本开放后）对敌方全体进行强力的攻击<宝具升级效果提升>-对[神性]特攻攻击<超蓄力威力提升>-敌方全体的Buster卡耐性下降（3回合）", "T_Num_Arr": [["400%", "500%", "550%", "575%", "600%"], ["150%", "162.5%", "175%", "187.5%", "200%"], ["20%"]], "S1LV": [["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["∅"]], "S2LV": [["20%", "21%", "22%", "23%", "24%", "25%", "26%", "27%", "28%", "30%"], ["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S3LV": [["25%"], ["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["20%", "22%", "24%", "26%", "28%", "30%", "32%", "34%", "36%", "40%"]], "Skill_Name_Arr": ["枪之辉石", "枪之辉石", "枪之魔石", "枪之魔石,无间的齿车", "枪之秘石,无间的齿车", "枪之秘石,八连双晶", "八连双晶,英雄之证", "英雄之证,蛮神的心脏", "传承结晶"], "Skill_Img_Arr": ["sphere03.png", "sphere03.png", "ruby03.png", "ruby03.png,gear.png", "star03.png,gear.png", "star03.png,cluster.png", "cluster.png,plate.png", "plate.png,heart.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Silver", "Silver,Copper", "Copper,Gold", "Gold"], "Skill_Number_Arr": ["5", "12", "5", "12,5", "5,10", "12,5", "10,18", "54,10", "1"], "Skill_QP_Arr": ["200000", "400000", "1200000", "1600000", "4000000", "5000000", "10000000", "12000000", "20000000