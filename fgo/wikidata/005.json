[{"ID": "5", "NAME": "尼禄·克劳狄乌斯", "NAME_JP": "ネロ・クラウディウス", "NAME_EN": "Nero Claudius", "PIC1": "005A.png", "PIC2": "005B.png", "PIC3": "005C.png", "PIC4": "005D.png", "PIC5": "005Z.png", "Avatar": "005", "STAR": "4", "CLASS": "Saber", "LV1_HP": "1880", "LV1_ATK": "1574", "LVMAX0_HP": "6816", "LVMAX0_ATK": "5511", "LVMAX1_HP": "7162", "LVMAX1_ATK": "5787", "LVMAX2_HP": "8218", "LVMAX2_ATK": "6629", "LVMAX3_HP": "9837", "LVMAX3_ATK": "7921", "LVMAX4_HP": "11753", "LVMAX4_ATK": "9449", "LV90_HP": "12996", "LV90_ATK": "10441", "LV100_HP": "14250", "LV100_ATK": "11441", "COST": "12", "ILLUST": "ワダアルコ", "CV": "丹下樱", "GLUTEN": "D", "DURABLE": "D", "AGILE": "A", "MAGIC": "B", "LUCKY": "A", "TREASURE": "B", "SKILL_R1": "头痛", "SKILL_P1": "401.png", "SKILL_L1": "B", "SKILL_C1": "7", "SKILL_E1": ["自身的精神耐性提升（3回合）", "HP回复"], "SKILL_R2": "皇帝特权", "SKILL_P2": "600.png", "SKILL_L2": "EX", "SKILL_C2": "7", "SKILL_E2": ["自身HP回复", "60%几率自身的攻击力提升（3回合）", "60%几率自身的防御力提升（3回合）"], "SKILL_R3": "纵使三度迎接夕阳", "SKILL_P3": "404.png", "SKILL_C3": "12", "SKILL_L3": "A", "SKILL_E3": ["赋予自身毅力状态（3次·5回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "C", "CSKILL_E1": "自身的弱体耐性少许提升（15%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "B", "CSKILL_E2": "自身的Quick卡性能提升（8%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "童女讴歌如花帝政（Los Saint Claudius）", "T_PROP": "Arts", "T_LEVEL": "B", "T_TYPE": "对阵宝具", "T_EFFECT": "对敌方全体进行强力的无视防御力的攻击&防御力下降（1回合）<超蓄力效果提升>", "CARD_BUSTER": "2", "CARD_ARTS": "2", "CARD_QUICK": "1", "INTRO1": "自称是男装的丽人。既自我至上主义又自私任性，明朗豁达，像小孩子一样天真无邪、被万人爱戴的无所不能恣意妄为的皇帝。\n本名，尼禄・克劳狄乌斯・凯撒・奥古斯都・日耳曼尼库斯。\n罗马帝国的第5代皇帝。生涯被涂上谋略与毒之色彩的恶名昭彰的暴君。", "INTRO2": "身高/体重：150cm・42kg\n出典：史实\n地域：罗马\n属性：混沌・善\u3000\u3000性別：女性\n嗯，全部交给余就好了！", "INTRO3": "自称是艺术家，有着独特的审美观。\n美貌的男子不论年纪都很喜欢，对于美丽的少女也十分有兴趣。\n只要是美丽的东西，什么都喜欢。\n将自己的一切讴歌为至高的艺术，也把各种各样的人生赞美为美妙之物。\n……虽然是无需多言的事，这样享受着人生的她的姿态本身也如同一朵盛开的蔷薇。", "INTRO4": "皇帝特权：EX\n本来不能持有的技能，也可以因为本人的主张而能在短时间内获得。与之符合的技能有骑乘、剑术、艺术、领导力、军略等。等级是A以上的情况下，连肉体层面的负荷（神性等）也能获得。", "INTRO5": "“招荡的黄金剧场”\n等级：B\n种类：对阵宝具\nAestus Domus Aurea。\n达成自己愿望的绝对皇帝圈。将生前的她自己设计并在罗马建造的剧场“Domus Aurea”，用魔力再现。作出只对她有利的战场。", "INTRO6": "将罗马的人们的幸福与繁荣考虑在第一位的皇帝。\n自己也是享受人生的达人，特别是在歌剧方面有着无人能出其右的自信。（也仅仅是自信而已）\n据说听过她的歌声的人，三天三夜都受其影响的折磨，连做梦做的好像都是见到了蔷薇的（恶）梦。", "INTRO7": "在西历50~60年代的罗马，被各种宗教势力与罗马元老院压制着的皇帝。由于罗马时代对元老院的态度、对基督教徒进行镇压的原因而被称为暴君。\n根据这个逸话，能与记述于旧约圣经中的毁灭都市的666之兽、巴比伦的大淫妇视为同一存在……？", "Property": "骑乘,人型,从者（被EA克制）,罗马,Saber脸,王", "Camp": "人", "Crit": "11.0%", "Death": "24.5%", "CritPr": "102", "InitiativeNp": "0.84%", "Passive": "3.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "2", "EXHit": "5", "Height": "150", "Weight": "42", "Origin": "史实", "Region": "罗马", "Attributes": "混沌・善", "Gender": "女性", "T_Text_Arr": "（宝具本开放后）对敌方全体进行强力的无视防御力的攻击<宝具升级效果提升>-防御力下降（1回合）<超蓄力效果提升>", "T_Num_Arr": [["600%", "750%", "825%", "862.5%", "900%"], ["20%", "25%", "30%", "35%", "40%"]], "S1LV": [["50%", "55%", "60%", "65%", "70%", "75%", "80%", "85%", "90%", "100%"], ["500", "650", "800", "950", "1100", "1250", "1400", "1550", "1700", "2000"]], "S2LV": [["1200", "1420", "1640", "1860", "2080", "2300", "2520", "2740", "2960", "3400"], ["22%", "24.2%", "26.4%", "28.6%", "30.8%", "33%", "35.2%", "37.4%", "39.6%", "44%"], ["22%", "24.2%", "26.4%", "28.6%", "30.8%", "33%", "35.2%", "37.4%", "39.6%", "44%"]], "S3LV": [["300", "330", "360", "390", "420", "450", "480", "510", "540", "600"]], "Skill_Name_Arr": ["剑之辉石", "剑之辉石", "剑之魔石", "剑之魔石,凤凰的羽毛", "剑之秘石,凤凰的羽毛", "剑之秘石,八连双晶", "八连双晶,龙牙", "龙牙,无间的齿车", "传承结晶"], "Skill_Img_Arr": ["sphere01.png", "sphere01.png", "ruby01.png", "ruby01.png,feather.png", "star01.png,feather.png", "star01.png,cluster.png", "cluster.png,tooth.png", "tooth.png,gear.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Silver", "Silver,Copper", "Copper,Silver", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10, 4", "4, 7", "10, 4", "8, 12", "36, 20", "1