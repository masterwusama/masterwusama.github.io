[{"ID": "98", "NAME": "库·丘林〔Alter〕", "NAME_JP": "クー・フーリン〔オルタ〕", "NAME_EN": "Cu Chulainn （Alter)", "PIC1": "098A.png", "PIC2": "098B.png", "PIC3": "098C.png", "PIC4": "098D.png", "PIC5": "098Z.png", "Avatar": "098", "STAR": "5", "CLASS": "Berserker", "LV1_HP": "1790", "LV1_ATK": "1979", "LVMAX0_HP": "7062", "LVMAX0_ATK": "7456", "LVMAX1_HP": "7666", "LVMAX1_ATK": "8084", "LVMAX2_HP": "8823", "LVMAX2_ATK": "9286", "LVMAX3_HP": "10407", "LVMAX3_ATK": "10932", "LVMAX4_HP": "12210", "LVMAX4_ATK": "12805", "LV90_HP": "12210", "LV90_ATK": "12805", "LV100_HP": "13377", "LV100_ATK": "14017", "COST": "16", "ILLUST": "しまどりる", "CV": "神奈延年", "GLUTEN": "A", "DURABLE": "B+", "AGILE": "A+", "MAGIC": "C", "LUCKY": "D", "TREASURE": "A", "SKILL_R1": "精灵的骚动", "SKILL_P1": "503.png", "SKILL_L1": "A", "SKILL_C1": "8", "SKILL_E1": ["敌方全体的暴击发生率降低（3回合）", "攻击力降低（3回合）"], "SKILL_R2": "避矢的加护", "SKILL_P2": "402.png", "SKILL_L2": "C", "SKILL_C2": "7", "SKILL_E2": ["赋予自身回避状态（2次）", "防御力提升（3回合）"], "SKILL_R3": "战斗续行", "SKILL_P3": "404.png", "SKILL_C3": "9", "SKILL_L3": "A", "SKILL_E3": ["赋予自身毅力状态（1次·5回合）"], "CSKILL_R1": "狂化", "CSKILL_P1": "106.png", "CSKILL_L1": "EX（相当于C)", "CSKILL_E1": "自身的Buster卡性能提升（6%)", "CSKILL_R2": "神性", "CSKILL_P2": "326.png", "CSKILL_L2": "C", "CSKILL_E2": "赋予自身伤害提升状态（150)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "啮碎死牙之兽（Curruid Coinchenn）", "T_PROP": "Buster", "T_LEVEL": "A", "T_TYPE": "对人宝具", "T_EFFECT": ["自己的攻击力提升（1回合）<超蓄力效果提升>&防御力提升（1回合）<超蓄力效果提升>", "对敌方单体进行超强力攻击"], "CARD_BUSTER": "3", "CARD_ARTS": "1", "CARD_QUICK": "1", "INTRO1": "凯尔特・阿尔斯特传说的勇士。\n是赤枝骑士团的一员，也是阿尔斯特的最强战士，因异界“影之国”的盟主斯卡哈所教授的无敌魔枪术而英名远扬。\n这个他，是与平常不一样的Berserker职阶现身的。\n因某种原因，全身的装备发生了变化，\n性格也发生了反转。表情冷酷，宝具的魔枪也变成了混杂着黑色的红色，充满着不详的气息。", "INTRO2": "身高/体重：185cm・72kg\n出处：凯尔特神话\n地域：欧洲\n属性：混沌・恶\u3000\u3000性别：男性\n符文魔术只用于强化自己的肉体。", "INTRO3": "○精灵的骚动：A\n库・丘林的呻吟声，\n使在地下熟睡的精灵们觉醒，\n是一种能够粉碎敌军士兵们精神的精神系的干涉技能。\n全体敌人的筋力与敏捷参数发生暂时性的下降。", "INTRO4": "这库・丘林因女王梅芙的愿望变化而成的姿态。\n作为狂王库・丘林的他侵蚀着美国大地。\n\n并非是以生前的狂战士状态，而是以原本Berserker完全不同的姿态现身。\n在接受圣杯的援助时，超越了Lancer或是Caster状态时的强度。", "INTRO5": "『剜穿鏖杀之枪』\n等级：B++\u3000类别：对军宝具\n范围：5-50  最大捕捉：100人\nGae Bolg。\n自动制导性魔枪导弹。\n是库・丘林原本的宝具。\n作为Alter的场合会不顾自己肉体的崩坏而进行全力投掷，相比通常召唤时，威力和有效范围均有提升。\n对全体敌人都有即死效果，即使没有即死也会给予巨大伤害。\n（因为符文魔术可以将“崩坏的肉体同时再生”，所以投掷时除了惊人的痛苦外，库丘林并不会受到伤害。）\n", "INTRO6": "『啮碎死牙之兽』\n等级：A\u3000类别：对人宝具（自己）\n范围：—  最大捕捉：1人\nCurruid Coinchenn。\n狂暴化库・丘林的愤怒，让作为魔枪Gae Bolg原型的红海的怪物・海兽Curruid的外骨骼暂时具象化，并将其像铠甲一般缠绕在身上。\n这是攻击型的骨制盔甲。穿着他可使耐久等级提升，筋力参数达到EX。\n发动这个宝具时，无法使用『剜穿鏖杀之枪』。", "INTRO7": "第五章中的灾厄级别的敌人。\n这样的话，不如将作为主人公\n身着神话礼装的某位Archer召唤出来，\n互相在谜之空间从周日早上开始大战，\n你意下如何？", "Property": "人型,神性,从者（被EA克制）,天/地从者,所爱之人", "Camp": "地", "Crit": "5.1%", "Death": "52.0%", "CritPr": "9", "InitiativeNp": "0.69%", "Passive": "5.0%", "ArtHit": "3", "BusHit": "3", "QuiHit": "4", "EXHit": "5", "Height": "185cm", "Weight": "72kg", "Origin": "凯尔特神话", "Region": "欧州", "Attributes": "混沌・恶", "Gender": "男性", "T_Text_Arr": ["自身攻击力提升（1回合）<超蓄力效果提升>", "自身防御力提升（1回合）<超蓄力效果提升>", "对敌方单体进行超强力的攻击<宝具升级效果提升>"], "T_Num_Arr": [["30%", "40%", "50%", "60%", "70%"], ["30%", "40%", "50%", "60%", "70%"], ["600%", "800%", "900%", "950%", "1000%"]], "S1LV": [["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S2LV": [["∅"], ["7%", "7.7%", "8.4%", "9.1%", "9.8%", "10.5%", "11.2%", "11.9%", "12.6%", "14%"]], "S3LV": [["1000", "1150", "1300", "1450", "1600", "1750", "1900", "2050", "2200", "2500"]], "Skill_Name_Arr": ["狂之辉石", "狂之辉石", "狂之魔石", "狂之魔石,混沌之爪", "狂之秘石,混沌之爪", "狂之秘石,凶骨", "凶骨,蛇之宝玉", "蛇之宝玉,虚影之尘", "传承结晶"], "Skill_Img_Arr": ["sphere06.png", "sphere06.png", "ruby06.png", "ruby06.png,claw.png", "star06.png,claw.png", "star06.png,bone.png", "bone.png,snake.png", "snake.png,powder.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Gold", "Gold,Gold", "Gold,Copper", "Copper,Silver", "Silver,Copper", "Gold"], "Skil