[{"ID": "68", "NAME": "冲田总司", "NAME_JP": "沖田総司", "NAME_EN": "Okita Souji", "PIC1": "068A.png", "PIC2": "068B.png", "PIC3": "068C.png", "PIC4": "068D.png", "PIC5": "068Z.png", "Avatar": "068", "STAR": "5", "CLASS": "Saber", "LV1_HP": "1939", "LV1_ATK": "1865", "LVMAX0_HP": "7649", "LVMAX0_ATK": "7027", "LVMAX1_HP": "8304", "LVMAX1_ATK": "7619", "LVMAX2_HP": "9557", "LVMAX2_ATK": "8752", "LVMAX3_HP": "11272", "LVMAX3_ATK": "10302", "LVMAX4_HP": "13225", "LVMAX4_ATK": "12068", "LV90_HP": "13225", "LV90_ATK": "12068", "LV100_HP": "14489", "LV100_ATK": "13210", "COST": "16", "ILLUST": "武内崇", "CV": "悠木碧", "GLUTEN": "C", "DURABLE": "E", "AGILE": "A+", "MAGIC": "E", "LUCKY": "D", "TREASURE": "C", "SKILL_R1": "缩地", "SKILL_P1": "304.png", "SKILL_L1": "B", "SKILL_C1": "7", "SKILL_E1": ["自身的Quick卡性能提升（1回合）"], "SKILL_R2": "病弱", "SKILL_P2": "311.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["赋予自身星星集中状态（1回合）"], "SKILL_R3": "心眼（伪）", "SKILL_P3": "402.png", "SKILL_C3": "8", "SKILL_L3": "A", "SKILL_E3": ["赋予自身回避状态（1回合）", "暴击威力大幅度提升（3回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "E", "CSKILL_E1": "自身的弱体耐性少许提升（10%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "E", "CSKILL_E2": "自身的Quick卡性能少许提升（2%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "无明三段突刺", "T_PROP": "Quick", "T_LEVEL": "-", "T_TYPE": "对人魔剑", "T_EFFECT": ["对敌方单体进行无视防御力的超强力攻击&防御力大幅下降（3回合）<超蓄力效果提升>"], "CARD_BUSTER": "2", "CARD_ARTS": "1", "CARD_QUICK": "2", "INTRO1": "以幕末京都为活动中心的治安组织、\n新选组的一番队队长、冲田总司。\n即便在作为剑客集团被恐惧着的新选组中\n冲田也是被誉为最强的天才剑士的存在。", "INTRO2": "身高/体重：158cm・45kg\n出典：史实\n地域：日本\n属性：中立・中庸\u3000\u3000\u3000性別：女性\n「嗯嗯，光炮是放不出来的」", "INTRO3": "身着时髦的和服，十五岁左右的可爱少女。为了隐藏过于显眼的本来装束，而使用了这套衣装。\n兼有冷酷的杀手脸和招小孩子喜欢的脸。", "INTRO4": "无明三段突刺\n对人魔剑。最大捕捉：1人。\n稀世的天才剑士、冲田总司的必杀魔剑。\n“壹之突刺”内包含了“贰之突刺”和“参之突刺”。\n由平正眼的架势发动，不是“几乎同时”而是“完全同时”施展出水平突刺，\n是由无与伦比的技巧和速度孕育出的秘剑。", "INTRO5": "三段突刺的瞬间，壹之突刺、贰之突刺、参之突刺在“同个位置”上“同时存在”。\n即使防住壹之突刺，同个位置也会被贰之突刺、参之突刺贯穿——因为存在着这样的矛盾，剑尖会引发局部的“事象饱和”。\n因此，实质上三段突刺是无法防御的剑戟。\n利用事象饱和，其在物体破坏方面也很优秀。", "INTRO6": "同时有着超绝技巧和天赋之才的真正的天才剑士，却以作为Saber职阶的低耐久力、低对魔力引人注目。\n这是生前的病痛、以及后世民众的印象所改写的结果，某种意义上承受了和“无辜的怪物”相似的诅咒。", "INTRO7": "寄予圣杯的愿望是“战斗到最后一刻”。完成生前未果的约定，是她的悲愿。\n在成为了英灵的现在，常为与同伴们一起战斗的事懊悔着，对于自己作为新选组队员的失格深信不疑。", "Property": "骑乘,人型,从者（被EA克制）,Saber脸", "Camp": "人", "Crit": "10.2%", "Death": "35.0%", "CritPr": "98", "InitiativeNp": "1.09%", "Passive": "3.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "5", "EXHit": "3", "Height": "158", "Weight": "45", "Origin": "史实", "Region": "日本", "Attributes": "中立・中庸", "Gender": "女性", "T_Text_Arr": "对敌方单体进行无视防御力的超强力攻击<宝具升级效果提升>-防御力大幅下降（3回合）<超蓄力效果提升>", "T_Num_Arr": [["1200%", "1600%", "1800%", "1900%", "2000%"], ["30%", "35%", "40%", "45%", "50%"]], "S1LV": [["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"]], "S2LV": [["500%", "550%", "600%", "650%", "700%", "750%", "800%", "850%", "900%", "1000%"]], "S3LV": [["∅"], ["20%", "22%", "24%", "26%", "28%", "30%", "32%", "34%", "36%", "40%"]], "Skill_Name_Arr": ["剑之辉石", "剑之辉石", "剑之魔石", "剑之魔石,龙牙", "剑之秘石,龙牙", "剑之秘石,虚影之尘", "虚影之尘,混沌之爪", "混沌之爪,凤凰的羽毛", "传承结晶"], "Skill_Img_Arr": ["sphere01.png", "sphere01.png", "ruby01.png", "ruby01.png,tooth.png", "star01.png,tooth.png", "star01.png,powder.png", "powder.png,claw.png", "claw.png,feather.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Copper", "Gold,Copper", "Gold,Copper", "Copper,Gold", "Gold,Silver", "Gold"], "Skill_Number_Arr": ["5", "12", "5", "12,12", "5,24", "12,10", "20,4", "11,20", "1"], "Skill_QP_Arr": ["200000", "400000", "1200000", "1600000", "4000000", "5000000", "10000000", "12000000", "20000000"], "Break_Name_Arr": [["银色剑士棋子"], ["银色剑士棋子", "虚影之尘"], ["金色剑士棋子", "无间的齿车", "龙牙"], ["金色剑士棋子", "凤凰的羽毛", "无间的齿车"]], "Break_Img_Arr": [["figure1s.png"], ["figure1s.png", "powder.png"], ["figure1g.png", "gear.png", "tooth.png"], ["figure1g.png", "feather.png", "gear.png"]], "Break_Border_Arr": [["Silver"],