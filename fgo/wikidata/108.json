[{"ID": "108", "NAME": "伊斯坎达尔", "NAME_JP": "イスカンダル", "NAME_EN": "Iskandar", "PIC1": "108A.png", "PIC2": "108B.png", "PIC3": "108C.png", "PIC4": "108D.png", "PIC5": "108Z.png", "Avatar": "108", "STAR": "5", "CLASS": "Rider", "LV1_HP": "1938", "LV1_ATK": "1786", "LVMAX0_HP": "7646", "LVMAX0_ATK": "6731", "LVMAX1_HP": "8300", "LVMAX1_ATK": "7298", "LVMAX2_HP": "9552", "LVMAX2_ATK": "8383", "LVMAX3_HP": "11267", "LVMAX3_ATK": "9869", "LVMAX4_HP": "13219", "LVMAX4_ATK": "11560", "LV90_HP": "13219", "LV90_ATK": "11560", "LV100_HP": "14482", "LV100_ATK": "12654", "COST": "16", "ILLUST": "武内崇", "CV": "大冢明夫", "GLUTEN": "B", "DURABLE": "A", "AGILE": "D", "MAGIC": "C", "LUCKY": "A+", "TREASURE": "A++", "SKILL_R1": "领导力", "SKILL_P1": "300.png", "SKILL_L1": "A", "SKILL_C1": "7", "SKILL_E1": ["己方全体的攻击力提升（3回合）"], "SKILL_R2": "军略", "SKILL_P2": "302.png", "SKILL_L2": "B", "SKILL_C2": "7", "SKILL_E2": ["己方全体的宝具威力提升（1回合）"], "SKILL_R3": "雷之征服者", "SKILL_P3": "306.png", "SKILL_C3": "8", "SKILL_L3": "EX", "SKILL_E3": ["自身的Buster卡性能提升（1回合）", "星星发生率提升（1回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "D", "CSKILL_E1": "自身的弱体耐性少许提升（12.5%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "A+", "CSKILL_E2": "自身的Quick卡性能提升（11%)", "CSKILL_R3": "神性", "CSKILL_P3": "326.png", "CSKILL_L3": "C", "CSKILL_E3": "赋予自身伤害提升状态（150)", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "王之军势（Ionioi Hetairoi）", "T_PROP": "Buster", "T_LEVEL": "EX", "T_TYPE": "对军宝具", "T_EFFECT": "对敌方全体进行强力攻击&防御力降低（3回合）<超蓄力效果提升>", "CARD_BUSTER": "2", "CARD_ARTS": "2", "CARD_QUICK": "1", "INTRO1": "伊斯坎达尔，是亚历山大三世在波斯语中的称呼，双角王这一别称也广为人知。\n因为东方远征，他的勇武传遍了整个伊斯兰世界，造就了许多在亚洲地区广为流传的英雄传说。", "INTRO2": "在20岁继承了马其顿的王座后，便压制了希腊的城邦并向西亚进攻。\n在埃及得到了法老王的地位，并将自诩强大的波斯王国攻破，建立起了远至印度的强大帝国。", "INTRO3": "那份征服的热情并非出于扩大领土的支配欲望，\n更多的是因为他那想要探求未知的世界的探险之心。\n他会使用当地的人才统治他征服的地域，自己也积极地推想安抚西亚文化的政策，为此打下了希腊主义文化的基础。", "INTRO4": "身高/体重：212cm・130kg\n出典：史实\n地域：马其顿\n属性：中立・善\u3000\u3000性別：男性\n征服王，胜之而不灭，霸之而不辱。", "INTRO5": "『王之军势』\n等级：EX  种类：对军宝具\nAionion Hetairoi\n召唤出来哦的固有结界，征服王的王牌。\n人正是最珍贵的财宝。和勇者们创下的无与伦比的功勋正是王的证明。\n他把生前所率领的近卫兵团以独立的从者身份接连召唤出来，通过数万的军势蹂躏对手。", "INTRO6": "在『王之军势』中，生前的伊斯坎达尔军团会被完整地召唤出来。\n超越了时空的、和臣子们的那份羁绊在他的宝具里得到了升华，这正是他王之道的象征。\n生前并非是以个人之力建下功勋的英雄，而是指挥军队去征战的英雄——这就是他宝具的由来。", "INTRO7": "在到达尽头之海俄刻阿诺斯的悲愿下，完成了旷古烁今的大远征的征服王，他的热情无论在哪个时代的圣杯战争中都不会熄灭。\n在他的眼中，连圣杯的奇迹根本不算什么，他只是想以世界的尽头为目标再度进军。那才是无尚的光荣。", "Property": "人型,骑乘,神性,从者（被EA克制）,所爱之人,王,希腊神话系男性", "Camp": "人", "Crit": "8.8%", "Death": "40.0%", "CritPr": "205", "InitiativeNp": "0.66% & 0.86%(幕间通关后)", "Passive": "3.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "3", "EXHit": "6", "Height": "212cm", "Weight": "130kg", "Origin": "史实", "Region": "欧洲", "Attributes": "中立・善", "Gender": "男性", "T_Text_Arr": "（宝具本开放后）对敌方全体进行强力的攻击<宝具升级效果提升>-防御力下降（3回合）<超蓄力效果提升>-暴击发生率下降（3回合）<超蓄力效果提升>", "T_Num_Arr": [["400%", "500%", "550%", "575%", "600%"], ["10%", "15%", "20%", "25%", "30%"], ["10%", "20%", "30%", "40%", "50%"]], "S1LV": [["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S2LV": [["9%", "9.9%", "10.8%", "11.7%", "12.6%", "13.5%", "14.4%", "15.3%", "16.2%", "18%"]], "S3LV": [["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["50%"]], "Skill_Name_Arr": ["骑之辉石", "骑之辉石", "骑之魔石", "骑之魔石,英雄之证", "骑之秘石,英雄之证", "骑之秘石,陨蹄铁", "陨蹄铁,世界树之种", "世界树之种,凤凰的羽毛", "传承结晶"], "Skill_Img_Arr": ["sphere05.png", "sphere05.png", "ruby05.png", "ruby05.png,plate.png", "star05.png,plate.png", "star05.png,clevis.png", "clevis.png,walnut.png", "walnut.png,feather.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Copper", "Gold,Copper", "Gold,Silver", "Silver,Silver", "Silver,Silver", "Gold"], "Skill_Number_Arr": ["5", "12", "5", "12,15", "5,29", "12,5", "10,8", "22,20", "1"], "Skill_QP_Arr": ["200000", "400000", "1200000", "1600000", "4000000", "5000000", "10000000", "12000000", "20000000"], "Break_Name_Arr": [["银色骑士棋子"], ["银色骑士棋子", "陨蹄铁"], ["金色骑士棋子", "八连双晶", "英雄之证"], ["金色骑士棋子", "八连双晶", "战马的幼角"]], "Break_Img_Arr": [["f