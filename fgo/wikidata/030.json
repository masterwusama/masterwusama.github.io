[{"ID": "30", "NAME": "玛尔达", "NAME_JP": "マルタ", "NAME_EN": "Marthe", "PIC1": "030A.png", "PIC2": "030B.png", "PIC3": "030C.png", "PIC4": "030D.png", "PIC5": "030Z.png", "Avatar": "030", "STAR": "4", "CLASS": "Rider", "LV1_HP": "2090", "LV1_ATK": "1335", "LVMAX0_HP": "7502", "LVMAX0_ATK": "4527", "LVMAX1_HP": "8896", "LVMAX1_ATK": "5475", "LVMAX2_HP": "10279", "LVMAX2_ATK": "6317", "LVMAX3_HP": "11673", "LVMAX3_ATK": "7165", "LVMAX4_HP": "13068", "LVMAX4_ATK": "8014", "LV90_HP": "14451", "LV90_ATK": "8855", "LV100_HP": "15845", "LV100_ATK": "9703", "COST": "12", "ILLUST": "坂本みねぢ", "CV": "早见沙织", "GLUTEN": "D", "DURABLE": "C", "AGILE": "B", "MAGIC": "A", "LUCKY": "A+", "TREASURE": "A+", "SKILL_R1": "信仰的加护", "SKILL_P1": "401.png", "SKILL_L1": "A", "SKILL_C1": "7", "SKILL_E1": ["自身的弱体耐性提升（3回合）", "HP大量回复"], "SKILL_R2": "奇迹", "SKILL_P2": "600.png", "SKILL_L2": "D+", "SKILL_C2": "8", "SKILL_E2": ["己方全体HP大量回复", "解除弱体状态"], "SKILL_R3": "圣女的誓约", "SKILL_P3": "504.png", "SKILL_C3": "7", "SKILL_L3": "C", "SKILL_E3": ["敌方单体强化状态解除", "防御力下降（3回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "A", "CSKILL_E1": "自身的弱体耐性提升（20%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "A++", "CSKILL_E2": "自身的Quick卡性能提升（11.5%)", "CSKILL_R3": "神性", "CSKILL_P3": "326.png", "CSKILL_L3": "C", "CSKILL_E3": "赋予自身伤害提升状态（150)", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "不觉爱的悲哀之龙啊（Tarasque）", "T_PROP": "Buster", "T_LEVEL": "EX", "T_TYPE": "对军宝具", "T_EFFECT": ["对敌方全体进行强力攻击&防御力大幅度下降（3回合）<超蓄力效果提升>"], "CARD_BUSTER": "1", "CARD_ARTS": "3", "CARD_QUICK": "1", "INTRO1": "镇服了恶龙塔拉斯克的一世纪的圣女。\n与妹妹和弟弟一起被传颂，成为人们信仰的救世主。\n拥有美丽的容貌，热情洋溢，完美无缺的人。\n——连恐怖的怪兽都为之倾倒的神圣的少女。", "INTRO2": "身高/体重：161cm・49kg\n出典：史实、新约圣书\n地域：欧洲\n属性：秩序・善\u3000\u3000性別：女性\n例外的能够骑乘龙种的Rider", "INTRO3": "救世主被处刑之后也不曾舍弃信仰，被时任的罗马总督流放也没有死去，而是受到神的恩宠而向着法国南部漂流。\n在罗讷河（Rhône）沿岸的奈鲁克（Nerluc）镇，与令人们感到痛苦的暴虐之龙对峙。", "INTRO4": "奇迹：D\n时而将不可能化为可能的奇迹。固有技能\n与星之开拓者技能有着相似的部分，但本质上是不同的东西。\n能够适用的事物也不同。", "INTRO5": "外表内心都是完美的圣女，由于妹妹的存在，基本上像姐姐风格的言行会比较多。\n温柔纯洁，稳重和蔼——仿佛没有一处不闪耀着的人。\n只是……特别是对和她成为亲密之人的对象来说，也有见到“不是圣女的马大”的可能性。", "INTRO6": "“不是圣女的马大”表情与言辞都会变得锐利，变得好动好胜，这是圣女以前“与弟弟妹妹一起生活的城里姑娘”时候的样子，但在这种言行之下她的内在仍然保持是圣女，主要是隔阂的开启与关闭（on-off）的区别。", "INTRO7": "圣女马大对不能成为救世主的圣杯没有任何索求。与曾经一样，作为Servant存于现世的时候也是个圣女。\n所以，想必会协助Master的行动，\n不过，仅限Master是走在正道之上。", "Property": "骑乘,人型,从者（被EA克制）,神性", "Camp": "人", "Crit": "9.0%", "Death": "30.0%", "CritPr": "205", "InitiativeNp": "1.58%", "Passive": "3.0%", "ArtHit": "1", "BusHit": "1", "QuiHit": "2", "EXHit": "3", "Height": "161", "Weight": "49", "Origin": "史实、新约圣书", "Region": "欧洲", "Attributes": "秩序・善", "Gender": "女性", "T_Text_Arr": "（宝具本开放后）自身的Buster卡提升（1回合）-对敌方全体进行强力的攻击<宝具升级效果提升>-防御力大幅度下降（3回合）<超蓄力效果提升>", "T_Num_Arr": [["20%"], ["400%", "500%", "550%", "575%", "600%"], ["20%", "25%", "30%", "35%", "40%"]], "S1LV": [["50%", "55%", "60%", "65%", "70%", "75%", "80%", "85%", "90%", "100%"], ["1000", "1150", "1300", "1450", "1600", "1750", "1900", "2050", "2200", "2500"]], "S2LV": [["1000", "1100", "1200", "1300", "1400", "1500", "1600", "1700", "1800", "2000"], ["∅"]], "S3LV": [["∅"], ["10%", "12%", "14%", "16%", "18%", "20%", "22%", "24%", "26%", "30%"]], "Skill_Name_Arr": ["骑之辉石", "骑之辉石", "骑之魔石", "骑之魔石,世界树之种", "骑之秘石,世界树之种", "骑之秘石,龙牙", "龙牙,凤凰的羽毛", "凤凰的羽毛,龙之逆鳞", "传承结晶"], "Skill_Img_Arr": ["sphere05.png", "sphere05.png", "ruby05.png", "ruby05.png,walnut.png", "star05.png,walnut.png", "star05.png,tooth.png", "tooth.png,feather.png", "feather.png,claw1.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Copper", "Copper,Silver", "Silver,Gold", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10, 5", "4, 10", "10, 10", "20, 4", "12, 8", "1"], "Skill_QP_Arr": ["100000", "200000", "600000", "800000", "2000000", "2500000", "5000000", "6000000", "10000000"], "Break_Name_Arr": [["银色骑士棋子"], ["银色骑士棋子", "龙牙"], ["金色骑士棋子", "世界树之种", "混沌之爪"], ["金色骑士棋子", "龙之逆鳞", "混沌之爪"]], "Break_Img_Arr": [["figure5s.png"], ["figure5s.png", "tooth.png"