[{"ID": "100", "NAME": "海伦娜·布拉瓦茨基", "NAME_JP": "エレナ・ブラヴァツキー", "NAME_EN": "Helena Blavatsky", "PIC1": "100A.png", "PIC2": "100B.png", "PIC3": "100C.png", "PIC4": "100D.png", "PIC5": "100Z.png", "Avatar": "100", "STAR": "4", "CLASS": "Caster", "LV1_HP": "1901", "LV1_ATK": "1438", "LVMAX0_HP": "6821", "LVMAX0_ATK": "4983", "LVMAX1_HP": "8089", "LVMAX1_ATK": "5896", "LVMAX2_HP": "9346", "LVMAX2_ATK": "6802", "LVMAX3_HP": "10614", "LVMAX3_ATK": "7715", "LVMAX4_HP": "11882", "LVMAX4_ATK": "8629", "LV90_HP": "13139", "LV90_ATK": "9535", "LV100_HP": "14407", "LV100_ATK": "10448", "COST": "12", "ILLUST": "松竜", "CV": "金元寿子", "GLUTEN": "E", "DURABLE": "E", "AGILE": "D", "MAGIC": "A", "LUCKY": "A", "TREASURE": "A", "SKILL_R1": "魔力同调", "SKILL_P1": "601.png", "SKILL_L1": "C", "SKILL_C1": "9", "SKILL_E1": ["己方全体的NP增加"], "SKILL_R2": "圣雄", "SKILL_P2": "605.png", "SKILL_L2": "A", "SKILL_C2": "10", "SKILL_E2": ["赋予自身每回合获得星星状态（5回合）", "一定概率的宝具威力提升50%（1回合）"], "SKILL_R3": "对未知的探求", "SKILL_P3": "317.png", "SKILL_C3": "9", "SKILL_L3": "B", "SKILL_E3": ["己方全体的Arts卡性能提升（3回合）", "Quick卡性能提升（3回合）", "Buster卡性能提升（3回合）"], "CSKILL_R1": "阵地作成", "CSKILL_P1": "104.png", "CSKILL_L1": "A", "CSKILL_E1": "自身的Arts卡性能提升（10%)", "CSKILL_R2": "道具作成", "CSKILL_P2": "103.png", "CSKILL_L2": "B", "CSKILL_E2": "自身的弱体成功率提升（8%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "金星神・火炎天主（Sanat Kumāra）", "T_PROP": "Arts", "T_LEVEL": "A+", "T_TYPE": "对军宝具", "T_EFFECT": "对敌方全体进行强力攻击&防御力降低（3回合）<超蓄力效果提升>&暴击发生率降低（3回合）<超蓄力效果提升>&弱体耐性降低（3回合）<超蓄力效果提升>", "CARD_BUSTER": "1", "CARD_ARTS": "3", "CARD_QUICK": "1", "INTRO1": "十九世纪的女性神秘学家，神智学的始祖。\n年轻的时候虽与俄罗斯贵族结婚，但她立刻选择了逃婚，为魔术世界献出了自己闪耀的一生。\n通称为布拉瓦茨基夫人。\n她坚信利莫里亚大陆真的存在，并埋头研究神秘主义，在与高等的存在“圣雄”及其集合体“指导灵”的接触中，获得了大量的智慧。\n说不定她已经看见了通向根源的可能性。", "INTRO2": "身高/体重：145cm・38kg\n出典：史实\n地域：欧洲\n属性：混沌・善\u3000\u3000\u3000性别：女性\n喜欢的国家是印度。讨厌的团体是SPR（英国的心灵现象研究会）。", "INTRO3": "因突然变异而诞生的天才魔术师。\n她将抵达根源的先导们称作“圣雄”和“指导灵”，并规定（设想）他们为高等的存在。\n海伦娜说自己经常能感知到他们的信息，不过其他人若想感知到相同之物，需要具备与她相同的才能（特殊的魔术回路）。\n\n也有人认为利莫里亚大陆和圣雄并不存在，她只是一个“对个人的世界进行解释的天才而已”。", "INTRO4": "身为英灵的她，是一位全方位型的魔术师，能够操纵神智学为基础的古代到近现代的全部魔术的海伦娜，自然是以Caster的身份现身。\n虽然努力地显示自己可以操纵古埃及的魔术和圣堂教会的洗礼咏唱，不过实际上，她对于古代的魔术并不是特别擅长呢。", "INTRO5": "○道具作成：B\n制作带有魔力的器具。\n布拉瓦茨基夫人以曾经的心腹——侍奉于自己的“奥尔科特大佐”为原型，\n制作了许许多多的小型自动人偶，代替侍女们帮她做各种事。\n顺带一提，这些人偶并不太适合战斗。", "INTRO6": "『金星神・火炎天主』\n等级：A  种类：对军宝具\n范围：1-50  最大捕捉：150人\nSanat Kumāra。\n不明飞行物体出现，对四周进行攻击。\n“它具有从金星飞向地球的神性，能在短时间内让某位地球创造神——Sanat Kumāra（护法魔王尊）的力量再现，是神智学的究极奥义！”\n这是她的解释，但飞行物体的身份仍旧不明。", "INTRO7": "她寄予圣杯的愿望似乎有很多。\n虽然她说过自己还没有决定好是哪一个，\n但实际是怎么回事就没人知道了。\n……谜团重重呢，布拉瓦茨基夫人。\n“神秘的女人不是很有魅力吗？\n  啊不对！真理只有一个，人类虽然无法察觉，但圣雄通晓一切哟！”\n", "Property": "人型,从者（被EA克制）", "Camp": "人", "Crit": "10.7%", "Death": "36.0%", "CritPr": "51", "InitiativeNp": "0.45%", "Passive": "3.0%", "ArtHit": "3", "BusHit": "1", "QuiHit": "6", "EXHit": "3", "Height": "145cm", "Weight": "38kg", "Origin": "史实", "Region": "欧州", "Attributes": "混沌・善", "Gender": "女性", "T_Text_Arr": "（宝具本开放后）对敌方全体进行无视防御力的强力攻击<宝具升级效果提升>-对敌方全体赋予防御力降低（3回合）<超蓄力效果提升>-对敌方全体赋予暴击发生率降低效果（3回合）<超蓄力效果提升>-对敌方全体赋予弱体耐性降低效果（3回合）<超蓄力效果提升>", "T_Num_Arr": [["600%", "750%", "825%", "862.5%", "900%"], ["10%", "20%", "30%", "40%", "50%"], ["10%", "20%", "30%", "40%", "50%"], ["10%", "20%", "30%", "40%", "50%"]], "S1LV": [["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S2LV": [["5"], ["60%", "62%", "64%", "66%", "68%", "70%", "72%", "74%", "76%", "80%"]], "S3LV": [["15%", "15.5%", "16%", "16.5%", "17%", "17.5%", "18%", "18.5%", "19%", "20%"], ["15%", "15.5%", "16%", "16.5%", "17%", "17.5%", "18%", "18.5%", "19%", "20%"], ["15%", "15.5%", "16%", "16.5%", "17%", "17.5%", "18%", "18.5%", "19%", "20%"]], "Skill_Name_Arr": ["术之辉石", "术之辉石", "术之魔石", "术之魔石,幽灵提灯", "术之秘石,幽灵提灯", "术之秘石,虚影之尘", "虚影之尘,黑兽脂", "黑兽脂,凤凰的羽毛", "传承结晶"], "Skill_Img_Arr": ["sphere04.png", "sphere04.png", "ruby04.png", "ruby04.png,lamp.png", "star04.png,lamp.png", "star04.png,powder.png", "powder.png,fat.png", "fat.p