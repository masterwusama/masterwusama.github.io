[{"ID": "78", "NAME": "阿尔托莉雅·潘德拉贡〔Alter〕", "NAME_JP": "アルトリア・ペンドラゴン〔オルタ〕", "NAME_EN": "Altria Pendragon （Alter)", "PIC1": "078A.png", "PIC2": "078B.png", "PIC3": "078C.png", "PIC4": "078D.png", "PIC5": "078Z.png", "Avatar": "078", "STAR": "4", "CLASS": "Lancer", "LV1_HP": "1881", "LV1_ATK": "1661", "LVMAX0_HP": "6732", "LVMAX0_ATK": "5739", "LVMAX1_HP": "8322", "LVMAX1_ATK": "7077", "LVMAX2_HP": "9765", "LVMAX2_ATK": "8289", "LVMAX3_HP": "10940", "LVMAX3_ATK": "9278", "LVMAX4_HP": "11761", "LVMAX4_ATK": "9968", "LV90_HP": "13005", "LV90_ATK": "11014", "LV100_HP": "14260", "LV100_ATK": "12069", "COST": "12", "ILLUST": "石田あきら", "CV": "川澄绫子", "GLUTEN": "A", "DURABLE": "A+", "AGILE": "C", "MAGIC": "A+", "LUCKY": "D", "TREASURE": "A++", "SKILL_R1": "魔力放出", "SKILL_P1": "306.png", "SKILL_L1": "A+", "SKILL_C1": "7", "SKILL_E1": ["自身的Buster卡性能提升（1回合）"], "SKILL_R2": "最遥远的加护", "SKILL_P2": "311.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["赋予自身星星集中状态（1回合）", "暴击威力提升（1回合）", "获得星星"], "SKILL_R3": "领导力", "SKILL_P3": "300.png", "SKILL_C3": "7", "SKILL_L3": "E", "SKILL_E3": ["己方全体的攻击力提升（3回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "B", "CSKILL_E1": "自身的弱体耐性提升（17.5%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "A", "CSKILL_E2": "自身的Quick卡性能提升（10%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "耀于至远之枪（Rhongomyniad）", "T_PROP": "Buster", "T_LEVEL": "A++", "T_TYPE": "对城宝具", "T_EFFECT": ["对敌方全体进行强力的贯通攻击&中概率赋予宝具封印状态（1回合）&赋予诅咒状态（5回合）"], "CARD_BUSTER": "2", "CARD_ARTS": "1", "CARD_QUICK": "2", "INTRO1": "传说中的圆桌骑士王亚瑟本人。\n实际上，她是一名惹人怜爱，\n但也如同传说兼备清廉与高洁的少女骑士。\n虽作为圣剑使广为人知，实际上她所拥有的圣枪也极为强力，\n因此也可以作为Lancer现界。\n——此外，有可能是受圣杯的诅咒所侵袭而现界。是骑士王作为枪使时冷酷无情的另一面。", "INTRO2": "身高/体重：171cm・57kg？\n出典：亚瑟王传说\n地域：欧洲\n属性：秩序・善\u3000\u3000\u3000性別：女性\n作为Lancer时的体重和体形有多种说法。\n此外，作为Lancer时，必定会骑马。Alter化时的坐骑叫“拉姆莱”。", "INTRO3": "“耀于至远之枪”\n等级：A++    种类：对城宝具\nRhongomyniad。圣枪。别名Rhon。其真实姿态，是连接世界表里之物。\n解放真名之际，等级和种类都会发生变化。", "INTRO4": "圣枪原本是，将世界的表层缝合的“光之柱”，\n若有万一使其被瓦解，现有的物理法则\n将从由此构成的世界中剥离而堕落，\n既往之物及大量的幻想法则将会出现。\n正因为是立于神代与幻想的终末的“王”，\n她才能拥有这把蕴含极大力量的圣枪。", "INTRO5": "藉由解放其真名，\n该圣枪得以释放于至远的尽头闪耀的光辉之力的一隅。\n要进行真名解放，必须解除合计十三道束缚中的至少半数。\n这一性质与“救世的星之圣剑”等同。", "INTRO6": "根据不同的说法，作为Lancer现界之际，\n骑士王的“一部分”姿态会和生前有所不同。\n由于不持有圣剑和剑鞘，而是持有圣枪的缘故，\n其不老的肉体的“一部分”得以成长，\n亦或是，枪的魔力使肉体的“一部分”发生成长——", "INTRO7": "即便作为圣剑的替代而持有着圣枪，骑士王的精神性不会发生改变。\n得到选定之剑由此成王，对抗侵袭而来的外敌、魔物甚至是龙，\n为了祖国不断战斗着的，正是这位骑士王。\n即使是作为Alter现界，\n在修正人类史之时，\n会成为Master的强大力量这件事是不会错的。", "Property": "骑乘,人型,龙,亚瑟,Saber脸,从者（被EA克制）,天/地从者,王", "Camp": "天", "Crit": "11.8%", "Death": "23.0%", "CritPr": "88", "InitiativeNp": "0.74%", "Passive": "4.0%", "ArtHit": "3", "BusHit": "1", "QuiHit": "2", "EXHit": "4", "Height": "171cm", "Weight": "57kg？", "Origin": "亚瑟王传说", "Region": "欧洲", "Attributes": "秩序・善", "Gender": "女性", "T_Text_Arr": ["（宝具本开放后）对敌方全体进行无视防御力的强力攻击<宝具升级效果提升>", "中概率赋予宝具封印状态（1回合）", "赋予诅咒状态（5回合）<超蓄力效果提升>"], "T_Num_Arr": [["400%", "500%", "550%", "575%", "600%"], ["60%"], ["1000", "1500", "2000", "2500", "3000"]], "S1LV": [["35%", "37%", "39%", "41%", "43%", "45%", "47%", "49%", "51%", "55%"]], "S2LV": [["500%", "550%", "600%", "650%", "700%", "750%", "800%", "850%", "900%", "1000%"], ["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["5", "6", "6", "7", "7", "8", "8", "9", "9", "10"]], "S3LV": [["6%", "6.6%", "7.2%", "7.8%", "8.4%", "9%", "9.6%", "10.2%", "10.8%", "12%"]], "Skill_Name_Arr": ["枪之辉石", "枪之辉石", "枪之魔石", "枪之魔石,凤凰的羽毛", "枪之秘石,凤凰的羽毛", "枪之秘石,陨蹄铁", "陨蹄铁,龙牙", "龙牙,蛮神的心脏", "传承结晶"], "Skill_Img_Arr": ["sphere03.png", "sphere03.png", "ruby03.png", "ruby03.png,feather.png", "star03.png,feather.png", "star03.png,clevis.png", "clevis.png,tooth.png", "tooth.png,heart.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Copper", "Copper,Silver", "Silver,Gold", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10,4", "4,7", "10,4", "8,12", "36,8", "1"], "Skill_QP