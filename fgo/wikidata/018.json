[{"ID": "18", "NAME": "伊丽莎白·巴陶里", "NAME_JP": "エリザベート・バートリー", "NAME_EN": "Erzsebet Bathory", "PIC1": "018A.png", "PIC2": "018B.png", "PIC3": "018C.png", "PIC4": "018D.png", "PIC5": "018Z.png", "Avatar": "018", "STAR": "4", "CLASS": "Lancer", "LV1_HP": "1899", "LV1_ATK": "1520", "LVMAX0_HP": "6814", "LVMAX0_ATK": "5267", "LVMAX1_HP": "8081", "LVMAX1_ATK": "6233", "LVMAX2_HP": "9337", "LVMAX2_ATK": "7191", "LVMAX3_HP": "10603", "LVMAX3_ATK": "8156", "LVMAX4_HP": "11870", "LVMAX4_ATK": "9122", "LV90_HP": "13126", "LV90_ATK": "10079", "LV100_HP": "14392", "LV100_ATK": "11045", "COST": "12", "ILLUST": "ワダアルコ", "CV": "大久保瑠美", "GLUTEN": "C", "DURABLE": "D", "AGILE": "E", "MAGIC": "A", "LUCKY": "B", "TREASURE": "D", "SKILL_R1": "嗜虐的领导力", "SKILL_P1": "300.png", "SKILL_L1": "C", "SKILL_C1": "7", "SKILL_E1": ["己方全体的攻击力提升（3回合）", "除自身外己方全体[女性]的攻击力提升（3回合）"], "SKILL_R2": "拷问技术", "SKILL_P2": "504.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["敌方单体的防御力下降（3回合）"], "SKILL_R3": "战斗续行", "SKILL_P3": "404.png", "SKILL_C3": "9", "SKILL_L3": "B", "SKILL_E3": ["赋予自身毅力状态（1次·4回合）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "A", "CSKILL_E1": "自身的弱体耐性提升（20%)", "CSKILL_R2": "阵地作成", "CSKILL_P2": "104.png", "CSKILL_L2": "B", "CSKILL_E2": "自身的Arts卡性能提升（8%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "鲜血魔娘（Báthory Erzsébet）", "T_PROP": "Buster", "T_LEVEL": "E+", "T_TYPE": "对人宝具", "T_EFFECT": "对敌方全体进行强力的无视防御力的攻击&赋予诅咒状态（3回合）<超蓄力效果提升>", "CARD_BUSTER": "2", "CARD_ARTS": "1", "CARD_QUICK": "2", "INTRO1": "吸血鬼卡米拉的原型的血之伯爵夫人。\n……但是，作为从者被召唤的是她尚未犯下罪行之前十四岁的姿态。\n自称偶像的甜食系从者。\n高贵、霸道、残忍、无情，但其实并不像传说里一样惨无人道。", "INTRO2": "身高/体重：154cm・44kg\n出典：史实\n地域：匈牙利\n属性：混沌・恶  性别：女性\n※全长不包括尾巴", "INTRO3": "虽然是邪恶的反英灵，却同时也是个向往恋爱的少女。性子上胆小的部分简直是灾难，结果上要么是帮了自己人，要么就是放跑了敌人，会在关键时刻“哎，怎么说也是个英灵嘛”地发起善心来。\n但她很讨厌被人说“其实是个好人来的吧？”之类的话。", "INTRO4": "龙之息吹：E\n身为最强幻想种的龙所放出的魔力（Mana）湍流。\n超音波龙息（Super Sonic Dragon Breath）。\n虽然因为技能“无辜的怪物”而龙化了，不过有点像是在勉强自己，威力低微。\n她半龙化的原因，大抵是出于巴托里家的纹章（龙的牙齿）的缘故。", "INTRO5": "“鲜血魔娘”\n等级：E-\n种类：对人宝具\n（Báthory Erzsébet）\n召唤出伊丽莎白终生在其中君临天下的居城・监狱城赛伊特，把其当成衬托自己的舞台的宝具。\n唱着达到了毁灭级的音程以外的歌，展开等同于地狱般的舞台。", "INTRO6": "领导力：C\n表示出作为支配阶级的威压。\n由于特殊的传承辅助，对于女性会上升一等级。面对男性的话奇妙地有些洁癖，虽然很喜欢隔着器具来触摸，但是直接触摸的情况很少。", "INTRO7": "尽管是生为贵族而没有一般人的道德观念，又因为技能“头痛”不把别人当人来看的伊丽莎白，这次也因为有所改悔、或者说是有所成长的关系，在本作成了还算懂事的乖乖反英灵。", "Property": "人型,从者（被EA克制）,龙", "Camp": "人", "Crit": "11.8%", "Death": "24.0%", "CritPr": "90", "InitiativeNp": "1.10%", "Passive": "4.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "2", "EXHit": "3", "Height": "154", "Weight": "44", "Origin": "史实", "Region": "匈牙利", "Attributes": "混沌・恶", "Gender": "女性", "T_Text_Arr": "（宝具本开放后）对敌方全体进行强力的无视防御力的攻击<宝具升级效果提升>-赋予诅咒状态（3回合）<超蓄力效果提升>-强化成功率下降（3回合）", "T_Num_Arr": [["400%", "500%", "550%", "575%", "600%"], ["500", "1000", "1500", "2000", "2500"], ["20%"]], "S1LV": [["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"], ["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S2LV": [["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S3LV": [["750", "875", "1000", "1125", "1250", "1375", "1500", "1625", "1750", "2000"]], "Skill_Name_Arr": ["枪之辉石", "枪之辉石", "枪之魔石", "枪之魔石,龙之逆鳞", "枪之秘石,龙之逆鳞", "枪之秘石,龙牙", "龙牙,禁断之页", "禁断之页,凤凰的羽毛", "传承结晶"], "Skill_Img_Arr": ["sphere03.png", "sphere03.png", "ruby03.png", "ruby03.png,claw1.png", "star03.png,claw1.png", "star03.png,tooth.png", "tooth.png,paper.png", "paper.png,feather.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Gold", "Gold,Gold", "Gold,Copper", "Copper,Silver", "Silver,Silver", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10, 2", "4, 4", "10, 10", "20, 5", "15, 16", "1"], "Skill_QP_Arr": ["100000", "200000", "600000", "800000", "2000000", "2500000", "5000000", "6000000", "10000000"], "Break_Name_Arr": [["银色枪兵棋子"], ["银色枪兵棋子", "龙牙"], ["金色枪兵棋子", "龙之逆鳞", "混沌之爪"], ["金色枪兵棋子", "凤凰的羽毛", "混沌之爪"]], "Break_Img_Arr": [["figure3s.png"], ["fi