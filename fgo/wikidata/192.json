[{"ID": "192", "NAME": "喀耳刻", "NAME_JP": "キルケー", "NAME_EN": "Circe", "PIC1": "192A.png", "PIC2": "192B.png", "PIC3": "192C.png", "PIC4": "192D.png", "PIC5": "192Z.png", "Avatar": "192", "STAR": "4", "CLASS": "Caster", "LV1_HP": "1960", "LV1_ATK": "1445", "LVMAX0_HP": "7105", "LVMAX0_ATK": "5058", "LVMAX1_HP": "7465", "LVMAX1_ATK": "5310", "LVMAX2_HP": "8566", "LVMAX2_ATK": "6084", "LVMAX3_HP": "10253", "LVMAX3_ATK": "7269", "LVMAX4_HP": "12250", "LVMAX4_ATK": "8671", "LV90_HP": "13546", "LV90_ATK": "9581", "LV100_HP": "14853", "LV100_ATK": "10499", "COST": "12", "ILLUST": "左", "CV": "茜屋日海夏", "GLUTEN": "E", "DURABLE": "B", "AGILE": "D", "MAGIC": "A+", "LUCKY": "A", "TREASURE": "C", "SKILL_R1": "高速神言", "SKILL_P1": "601.png", "SKILL_L1": "B", "SKILL_C1": "9", "SKILL_E1": ["自身的NP超大幅度增加"], "SKILL_R2": "毒之食馔", "SKILL_P2": "538.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["赋予敌方全体[毒]状态（3回合）", "防御力下降（3回合）"], "SKILL_R3": "出航的助言", "SKILL_P3": "604.png", "SKILL_C3": "8", "SKILL_L3": "A", "SKILL_E3": ["己方全体的星星发生率提升（3回合）", "解除弱体状态"], "CSKILL_R1": "阵地作成", "CSKILL_P1": "104.png", "CSKILL_L1": "B", "CSKILL_E1": "自身的Arts卡性能提升（8%）", "CSKILL_R2": "道具作成", "CSKILL_P2": "103.png", "CSKILL_L2": "A", "CSKILL_E2": "自身的弱体赋予成功率提升（10%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "禁断之狂宴（Plump Piglets）", "T_PROP": "Buster", "T_LEVEL": "C", "T_TYPE": "对人宝具", "T_EFFECT": "", "CARD_BUSTER": "1", "CARD_ARTS": "3", "CARD_QUICK": "1", "INTRO1": "神话中著名的诱惑与堕落的魔女。\n极易陷入爱河，嫉妒心强，是永远的少女。\n楚楚可怜让人想溺爱的模样，搭配其让人沉醉的甜言蜜语，正是禁断之蜜的滋味。\n但是，倘若你还想当人类，你就要注意称呼她的方式......", "INTRO2": "身高/体重：147cm・39kg\n出典：希腊神话\n地域：欧洲\n属性：混沌・中庸\u3000\u3000性別：女性\n鹰之羽翼既衣服，也是流通着魔术回路的身体的一部分。", "INTRO3": "登场于荷马的叙事诗『奥德赛』，艾尤岛神话内的著名魔女。\n是一位与女神赫卡忒有血缘关系，掌管月与爱的半神。\n也是一个擅长调和具有各种效果让人变异的魔法药的理系女子。\n她会盛情款待来到岛上的人类男子，但在他们吃饱后，就会把他们变成狮子，狼以及猪等动物。\n她的哥哥是埃厄忒斯（美狄亚的父亲・科尔基斯王）。妹妹是帕西淮（阿斯忒里俄斯的母亲・克里特岛米诺斯王的妻子）。", "INTRO4": "『禁断之狂宴』\n等级：C\u3000种类：对人宝具\nPlump Piglets。\n召唤出的固有结界。\n被招待进豪华酒宴空间的不懂风情之徒，都会被魔女喀耳刻的爱猪“Piglets”蹂躏。\n这是一场愉快热闹的，能让你吃到肚子爆炸的恐怖魔术之宴。记住这股莫大喜悦的人，都会自主的希望能变成她的爱猪。", "INTRO5": "喜欢的东西：\n喜欢依赖自己，拥有缺点的人类男性。吃麦粥“kykeon”。\n讨厌的东西：\n被男性厌倦，被单方面的保护。\n\n连荷马也赞不绝口的，她那引以为傲的美丽头发某段时期变得很短，也是因为她大受打击的痛苦失恋经验。", "INTRO6": "喀耳刻在海神格劳科斯向其求助恋爱相关的问题时，就对格劳科斯一见钟情。但是她的爱却被格劳科斯拒绝了。嫉妒心很高的她，就把情敌水仙女斯库拉变成了怪物。之后，斯库拉就把奥德修斯的六名部下给吃掉了。\n\n英雄奥德修斯在返回故乡的途中，暂时在艾尤岛岛休息调整。虽然喀耳刻拿出了带毒的麦粥款待他，但因赫尔墨斯的加护，他打破了魔法，赢得了喀耳刻的芳心。她在奥德修斯离开的时候，给予了他很多避开旅途上艰难险阻的建议。", "INTRO7": "完成某任务后将解锁", "Property": "人型,从者（被EA克制）,天/地从者", "Camp": "天", "Crit": "10.8%", "Death": "34.5%", "CritPr": "51", "InitiativeNp": "0.39%", "Passive": "3.0%", "ArtHit": "3", "BusHit": "2", "QuiHit": "3", "EXHit": "4", "Height": "147cm", "Weight": "39kg", "Origin": "希腊神话", "Region": "欧洲", "Attributes": "混沌・中庸", "Gender": "女性", "T_Text_Arr": "对敌方单体进行超强力攻击<宝具升级效果提升>-赋予[猪化]状态<特殊的眩晕状态>(对一部分特别的敌人无效)（1回合）-暴击发生率下降（3回合）<超蓄力效果提升>", "T_Num_Arr": [["600%", "800%", "900%", "950%", "1000%"], ["100%"], ["10%", "20%", "30%", "40%", "50%"]], "S1LV": [["60%", "66%", "72%", "78%", "84%", "90%", "96%", "102%", "108%", "120%"]], "S2LV": [["500", "550", "600", "650", "700", "750", "800", "850", "900", "1000"], ["10%", "11%", "12%", "13%", "14%", "15%", "16%", "17%", "18%", "20%"]], "S3LV": [["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["∅"]], "Skill_Name_Arr": ["術の輝石", "術の輝石", "術の魔石", "術の魔石,原初の産毛", "術の秘石,原初の産毛", "術の秘石,禁断の頁", "禁断の頁,宵哭きの鉄杭", "宵哭きの鉄杭,蛮神の心臓", "伝承結晶"], "Skill_Img_Arr": ["sphere04.png", "sphere04.png", "ruby04.png", "ruby04.png,yinmao.png", "star04.png,yinmao.png", "star04.png,paper.png", "paper.png,zhui.png", "zhui.png,heart.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Gold", "Gold,Gold", "Gold,Silver", "Silver,Copper", "Copper,Gold", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10,3", "4,5", "10,4", "8,15", "45,8", "1"], "Skill_QP_Arr": ["100000", "200000", "600000", "800000", "2000000", "2500000", "5000000", "6000000", "10000000"], "Break_Name_Arr": [["キャスターピ