[{"ID": "160", "NAME": "亚瑟·潘德拉贡〔Prototype〕", "NAME_JP": "アーサー・ペンドラゴン〔プロトタイプ〕", "NAME_EN": "Arthur Pendragon〔Prototype〕", "PIC1": "160A.png", "PIC2": "160B.png", "PIC3": "160C.png", "PIC4": "160D.png", "PIC5": "160Z.png", "Avatar": "160", "STAR": "5", "CLASS": "Saber", "LV1_HP": "2049", "LV1_ATK": "1926", "LVMAX0_HP": "8608", "LVMAX0_ATK": "7722", "LVMAX1_HP": "9944", "LVMAX1_ATK": "8902", "LVMAX2_HP": "11291", "LVMAX2_ATK": "10093", "LVMAX3_HP": "12627", "LVMAX3_ATK": "11274", "LVMAX4_HP": "13975", "LVMAX4_ATK": "12465", "LV90_HP": "13975", "LV90_ATK": "12465", "LV100_HP": "15310", "LV100_ATK": "13645", "COST": "16", "ILLUST": "中原", "CV": "樱井孝宏", "GLUTEN": "A", "DURABLE": "A+", "AGILE": "B", "MAGIC": "B", "LUCKY": "B", "TREASURE": "EX", "SKILL_R1": "魔力放出", "SKILL_P1": "306.png", "SKILL_L1": "A", "SKILL_C1": "7", "SKILL_E1": ["自身的Buster卡性能提升（1回合）"], "SKILL_R2": "直感", "SKILL_P2": "603.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["获得大量星星"], "SKILL_R3": "巨兽狩猎", "SKILL_P3": "301.png", "SKILL_C3": "7", "SKILL_L3": "A", "SKILL_E3": ["赋予自身[超巨大]特攻状态（1回合）", "NP增加"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "A", "CSKILL_E1": "自身的弱体耐性提升（20%)", "CSKILL_R2": "骑乘", "CSKILL_P2": "102.png", "CSKILL_L2": "B", "CSKILL_E2": "自身的Quick卡性能提升（8%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "誓约胜利之剑（Excalibur）", "T_PROP": "Buster", "T_LEVEL": "EX", "T_TYPE": "？？？", "T_EFFECT": ["自身的宝具威力提升（1回合)<超蓄力效果提升>&对敌方全体进行强力攻击"], "CARD_BUSTER": "2", "CARD_ARTS": "2", "CARD_QUICK": "1", "INTRO1": "将良善之辈奉为瑰宝，对恶逆之事追究到底。\n所有童心未泯都曾经梦想过的，如同白马骑士一般的英雄。\n他也被称作星之圣剑使。\n然而他的真正身份，却是追随着某种存在辗转来到\n这个世界的异世界的“骑士王”。", "INTRO2": "身长/体重：181cm·68kg\n出典：亚瑟王传说\n地域：欧洲\n属性：秩序·善\u3000\u3000\u3000性别：男\n虽能很擅长吃但同样地也擅长做（料理）", "INTRO3": "与英灵阿尔托莉雅·潘德拉贡有着同样的过去与\n传说，二人既是完全一样的人物却又并非一人。\n是以可被称为「理想的王子大人」「苍银之骑士」\n这种清廉的姿态现世的。\n虽是如此，但据说他是自异世界来访的英灵——", "INTRO4": "〇领导力：B\n能够指挥军团的天生的才能。\n这次的亚瑟对巨兽作战能力进行了特别强化。\n因为与巨兽对峙时只得自身一人\n同伴无法靠近的缘故，所以并没有发挥领导力的余地。\n\n〇巨兽狩猎：A\n面对蹂躏了不列颠的魔兽们，亚瑟王展开了\n与它们的战斗，并将它们全部打倒了。\n是能够展示自身在面对巨大的敌性生物时\n有着丰富战斗经验的技能。", "INTRO5": "『誓约胜利之剑』\n等级：EX\u3000种类：？？？\nExcalibur。\n拯救了星球的光辉圣剑。\n是为了打倒想要毁灭星球的敌人而制造出的、\n击退一切罪恶的黄金之刃。\n\n这是将约束圣剑的“十三道束缚”中的六道解除后的姿态。\n因未达到解除半数以上——七道束缚的状态，\n所以还未能发挥它的真正实力。\n即便如此，也是足以清楚巨恶的苛烈之光。\n\n本作中“贝狄威尔束缚”的开放认可是\n自动进行的，\n就好像是预计到了要和巨大之物展开决战一样。", "INTRO6": "自己本来是属于异世界的英灵，\n为了追赶某个强大的敌人、不祥的预兆\n辗转来到了这个世界——其本人如是说。\n虽然这番话脱口便震惊全场，\n而且有着即使用迦勒底的程序也无法判断其真伪的性质，\n但至少他绝对不会对Master说谎。\n\n毕竟他，对Master寄托了所有的信赖，\n绝无任何虚伪与背叛之心。", "INTRO7": "完成某任务后将解锁", "Property": "骑乘,人型,从者（被EA克制）,龙,亚瑟,天/地从者,所爱之人,王", "Camp": "地", "Crit": "10.0%", "Death": "24.5%", "CritPr": "100", "InitiativeNp": "0.84%", "Passive": "3.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "3", "EXHit": "5", "Height": "181cm", "Weight": "68kg", "Origin": "亚瑟王传说", "Region": "欧洲", "Attributes": "秩序・善", "Gender": "男性", "T_Text_Arr": ["自身的宝具威力提升（1回合)<超蓄力效果提升>", "对敌方全体进行强力攻击<宝具升级效果提升>"], "T_Num_Arr": [["10%", "20%", "30%", "40%", "50%"], ["300%", "400%", "450%", "475%", "500%"]], "S1LV": [["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"]], "S2LV": [["5", "6", "7", "8", "9", "10", "11", "12", "13", "15"]], "S3LV": [["50%", "55%", "60%", "65%", "70%", "75%", "80%", "85%", "90%", "100%"], ["20%"]], "Skill_Name_Arr": ["剑之辉石", "剑之辉石", "剑之魔石", "剑之魔石,英雄之证", "剑之秘石,英雄之证", "剑之秘石,大骑士勋章", "大骑士勋章,原初的产毛", "原初的产毛,精灵根", "传承结晶"], "Skill_Img_Arr": ["sphere01.png", "sphere01.png", "ruby01.png", "ruby01.png,plate.png", "star01.png,plate.png", "star01.png,badge.png", "badge.png,yinmao.png", "yinmao.png,root.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Copper", "Gold,Copper", "Gold,Silver", "Silver,Gold", "Gold,Gold", "Gold"], "Skill_Number_Arr": ["5", "12", "5", "12,15", "5,29", "12,10", "20,4", "11,10", "1"], "Skill_QP_Arr": ["200000", "400000", "1200000", "1600000", "4000000", "5000000", "10000000", "12000000", "20000000"], "Break_Name_Arr": [["银色剑士棋子"], ["银色剑士棋子", "大骑士勋章"], ["金色剑士棋子", "英