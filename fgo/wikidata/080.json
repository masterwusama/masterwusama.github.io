[{"ID": "80", "NAME": "查尔斯·巴贝奇", "NAME_JP": "チャールズ・バベッジ", "NAME_EN": "Charles Babbage", "PIC1": "080A.png", "PIC2": "080B.png", "PIC3": "080C.png", "PIC4": "080D.png", "PIC5": "080Z.png", "Avatar": "080", "STAR": "3", "CLASS": "Caster", "LV1_HP": "1959", "LV1_ATK": "1113", "LVMAX0_HP": "5512", "LVMAX0_ATK": "3056", "LVMAX1_HP": "7521", "LVMAX1_ATK": "4155", "LVMAX2_HP": "9315", "LVMAX2_ATK": "5136", "LVMAX3_HP": "10512", "LVMAX3_ATK": "5790", "LVMAX4_HP": "10887", "LVMAX4_ATK": "5996", "LV90_HP": "13467", "LV90_ATK": "7407", "LV100_HP": "14761", "LV100_ATK": "8115", "COST": "7", "ILLUST": "I-IV", "CV": "稻田彻", "GLUTEN": "B++", "DURABLE": "B++", "AGILE": "D++", "MAGIC": "A", "LUCKY": "E", "TREASURE": "A+", "SKILL_R1": "一心一意", "SKILL_P1": "602.png", "SKILL_L1": "C", "SKILL_C1": "8", "SKILL_E1": ["自身的NP获得量提升（3回合）", "赋予星星集中状态（1回合）"], "SKILL_R2": "机关之铠", "SKILL_P2": "300.png", "SKILL_L2": "EX", "SKILL_C2": "7", "SKILL_E2": ["自身的攻击力提升（3回合）", "赋予自身无敌状态（1回合）"], "SKILL_R3": "过载", "SKILL_P3": "302.png", "SKILL_C3": "7", "SKILL_L3": "D", "SKILL_E3": ["自身的宝具威力大幅提升（1回合）", "赋予灼伤状态（5回合）【副作用】"], "CSKILL_R1": "道具作成（伪)", "CSKILL_P1": "103.png", "CSKILL_L1": "A", "CSKILL_E1": "自身的弱体成功率提升（10%)", "CSKILL_R2": "", "CSKILL_P2": "", "CSKILL_L2": "", "CSKILL_E2": "", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "绚烂的灰烬世界（Dimension of Steam）", "T_PROP": "Buster", "T_LEVEL": "A+", "T_TYPE": "对军宝具", "T_EFFECT": ["对敌方全体进行强力攻击&防御力下降（3回合）<超蓄力效果提升>"], "CARD_BUSTER": "2", "CARD_ARTS": "2", "CARD_QUICK": "1", "INTRO1": "十九世纪的数学家、科学家。\n梦想着世界变革的蒸汽王。\n世界上第一个利用蒸汽机关设计出计算机\n的“阶差机关”与“解析机关”的天才而博学之人。\n在现代被称为“计算机之父”。", "INTRO2": "身高/体重：250cm・500kg以上（均包含铠甲）\n出典：史实\n地域：欧州\n属性：混沌・中立\u3000\u3000性別：男性\n性格严肃，不擅长应付无邪或是聪明的少女。", "INTRO3": "——志未成而身先死。\n差分机没能完成。\n分析机没能完成。\n将消亡于时间的夹缝中的“可能的未来”的梦想\n残留于世，他死去了。", "INTRO4": "而如今，他再度现界。\n可能的未来，作为异形的钢铁缠绕全身。\n梦想的未来，作为异形的世界深埋心中。\n作为英灵现界的他，连同存在本身，\n都几乎与自己的宝具——固有结界相互同一化。", "INTRO5": "机关之铠：EX\n时常缠身的蒸汽机制成的全身机械铠甲。\n使筋力与耐久力等级上升的同时，\n由于异形的蒸汽机存有的增长（Boost）机能，\n为三个能力值赋予了“++”的补正。", "INTRO6": "“绚烂的灰烬世界”\n等级：A+ 种类：对军宝具\nDimension of Steam。\n既是由渴望与梦想之类升华而成的固有结界，\n也是他的心，是缠绕全身的机械铠其本身。", "INTRO7": "寄予圣杯的愿望是“蒸汽文明世界的到来”。\n只是，因为现界时便存在于现代的世界，\n多少获得了一些知识，而逐渐开始认识到这样也不错。\n对Master而言，他一定是个可以依赖的伙伴吧！", "Property": "人型,从者（被EA克制）", "Camp": "人", "Crit": "10.8%", "Death": "36.0%", "CritPr": "48", "InitiativeNp": "0.91%", "Passive": "3.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "2", "EXHit": "3", "Height": "250", "Weight": "500", "Origin": "史实", "Region": "欧洲", "Attributes": "混沌・中庸", "Gender": "男性", "T_Text_Arr": ["（宝具本开放后）对敌方全体进行强力的攻击<宝具升级效果提升>", "敌方全体防御力下降（3回合）<超蓄力效果提升>"], "T_Num_Arr": [["400%", "500%", "550%", "575%", "600%"], ["10%", "12.5%", "15%", "17.5%", "20%"]], "S1LV": [["20%", "21%", "22%", "23%", "24%", "25%", "26%", "27%", "28%", "30%"], ["500%", "550%", "600%", "650%", "700%", "750%", "800%", "850%", "900%", "1000%"]], "S2LV": [["20%", "22%", "24%", "26%", "28%", "30%", "32%", "34%", "36%", "40%"], ["∅"]], "S3LV": [["15%", "16%", "17%", "18%", "19%", "20%", "21%", "22%", "23%", "25%"], ["300"]], "Skill_Name_Arr": ["术之辉石", "术之辉石", "术之魔石", "术之魔石,八连双晶", "术之秘石,八连双晶", "术之秘石,无间的齿车", "无间的齿车,幽灵提灯", "幽灵提灯,虚影之尘", "传承结晶"], "Skill_Img_Arr": ["sphere04.png", "sphere04.png", "ruby04.png", "ruby04.png,cluster.png", "star04.png,cluster.png", "star04.png,gear.png", "gear.png,lamp.png", "lamp.png,powder.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Silver", "Silver,Copper", "Copper,Copper", "Gold"], "Skill_Number_Arr": ["4", "8", "4", "8,4", "4,7", "8,4", "7,4", "12,32", "1"], "Skill_QP_Arr": ["50000", "100000", "300000", "400000", "1000000", "1250000", "2500000", "3000000", "5000000"], "Break_Name_Arr": [["银色法师棋子"], ["银色法师棋子", "无间的齿车"], ["金色法师棋子", "八连双晶", "禁断之页"], ["金色法师棋子", "禁断之页", "虚影之尘"]], "Break_Img_Arr": [["figure4s.png"], ["figure4s.png", "gear.png"], ["figure4g.png", "cluster.png", "paper.png"], ["figure4g.p