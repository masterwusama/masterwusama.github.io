[{"ID": "43", "NAME": "夏尔·亨利·桑松", "NAME_JP": "シャルル＝アンリ・サンソン", "NAME_EN": "Charles Henri Sanson", "PIC1": "043A.png", "PIC2": "043B.png", "PIC3": "043C.png", "PIC4": "043D.png", "PIC5": "043Z.png", "Avatar": "043", "STAR": "2", "CLASS": "Assassin", "LV1_HP": "1564", "LV1_ATK": "968", "LVMAX0_HP": "3796", "LVMAX0_ATK": "2453", "LVMAX1_HP": "5428", "LVMAX1_ATK": "3539", "LVMAX2_HP": "6939", "LVMAX2_ATK": "4544", "LVMAX3_HP": "7978", "LVMAX3_ATK": "5236", "LVMAX4_HP": "8309", "LVMAX4_ATK": "5456", "LV90_HP": "10939", "LV90_ATK": "7206", "LV100_HP": "11991", "LV100_ATK": "7906", "COST": "5", "ILLUST": "しまどりる", "CV": "宫野真守", "GLUTEN": "D", "DURABLE": "D", "AGILE": "C", "MAGIC": "D", "LUCKY": "A", "TREASURE": "B", "SKILL_R1": "处刑人", "SKILL_P1": "301.png", "SKILL_L1": "A++", "SKILL_C1": "7", "SKILL_E1": "赋予自身[恶]特攻状态（3回合）", "SKILL_R2": "医术", "SKILL_P2": "600.png", "SKILL_L2": "A+", "SKILL_C2": "7", "SKILL_E2": "己方单体HP大量回复&解除弱体状态", "SKILL_R3": "人体研究", "SKILL_P3": "301.png", "SKILL_C3": "7", "SKILL_L3": "B", "SKILL_E3": "赋予自身[人类]特攻状态（3回合）", "CSKILL_R1": "气息遮断", "CSKILL_P1": "105.png", "CSKILL_L1": "D", "CSKILL_E1": "自身的星星发生率少许提升（4%)", "CSKILL_R2": "", "CSKILL_P2": "", "CSKILL_L2": "", "CSKILL_E2": "", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "死是向着明日的希望（la Molé Espoir）", "T_PROP": "Buster", "T_LEVEL": "A+", "T_TYPE": "对人宝具", "T_EFFECT": "对敌方单体进行超强力攻击&低概率即死效果<超蓄力概率提升>&防御力下降（3回合）<超蓄力效果提升>", "CARD_BUSTER": "1", "CARD_ARTS": "1", "CARD_QUICK": "3", "INTRO1": "世代在巴黎司死刑行刑的桑松家族第四代传人。\n在法国大革命这一激昂的时代当中，曾对不同阶层的人都进行过处刑。\n在他出于人道的考虑冥思苦想创造出的行刑工具“断头台（Guillotine）”亮相之后，\n经他的手进行的处刑却反而增多了。", "INTRO2": "身高/体重：178cm・68kg\n出典：史实\n地域：法国\n属性：秩序・恶\u3000\u3000性別：男性\n副业是医者，这点从技能中也能够看出来。", "INTRO3": "夏尔・亨利・桑松并不是暗杀者，而是处刑人。处刑人有世代继承的传统，他是第四代传人，虽然家境非常富裕，但是因为职业的缘故被蔑视也是常事，年轻的夏尔对此很是苦恼。", "INTRO4": "作为贵族又过着优雅生活的桑松家，敬爱着国王与王妃，是他们治理的国民中格外仁慈的家族，也会将为了处刑而培养出的最尖端的医疗技术，无偿提供给贫穷的人们。", "INTRO5": "但是，时代并不允许夏尔过上平稳的生活。尤其是因为法国革命的爆发，被强加了处刑者工作的他，不久将会面临进行自己最敬爱的法国国王路易十六和他的妃子玛丽・安托瓦内特的处刑。", "INTRO6": "“死是向着明日的希望”\n等级：A\n种类：对人宝具\n真正的处刑道具，断头台的具现化。对死亡概率的判定看的不是对诅咒的抵抗力和幸运，而是取决于是否有打破命运的强大精神。", "INTRO7": "夏尔・亨利・桑松对Master保持着中立的立场，被召唤的理由果然是希望自己这双杀人的手能得到赏识，心中也有着一些复杂的情感。", "Property": "人型,从者（被EA克制）", "Camp": "人", "Crit": "24.8%", "Death": "49.5%", "CritPr": "102", "InitiativeNp": "1.06%", "Passive": "4.0%", "ArtHit": "2", "BusHit": "1", "QuiHit": "2", "EXHit": "3", "Height": "178", "Weight": "68", "Origin": "史实", "Region": "法国", "Attributes": "秩序・恶", "Gender": "男性", "T_Text_Arr": "（宝具本开放后）概率即死效果<超蓄力效果提升>-对敌方单体进行超强力的攻击<宝具升级效果提升>-防御力下降（3回合）<超蓄力效果提升>", "T_Num_Arr": "30%|42.5%|55%|67.5%|80%-800%|1000%|1100%|1150%|1200%-30%|35%|40%|45%|50%", "S1LV": "40%|43%|44%|46%|48%|50%|52%|54%|56%|60%", "S2LV": "1000|1200|1400|1600|1800|2000|2200|2400|2600|3000-∅", "S3LV": "40%|42%|44%|46%|48%|50%|52%|54%|56%|60%", "Skill_Name_Arr": "杀之辉石|杀之辉石|杀之魔石|杀之魔石,虚影之尘|杀之秘石,虚影之尘|杀之秘石,凶骨|凶骨,禁断之页|禁断之页,人造人幼体|传承结晶", "Skill_Img_Arr": "sphere07.png|sphere07.png|ruby07.png|ruby07.png,powder.png|star07.png,powder.png|star07.png,bone.png|bone.png,paper.png|paper.png,potion.png|cubes.png", "Skill_Border_Arr": "Copper|Copper|Silver|Silver,Copper|Gold,Copper|Gold,Copper|Copper,Silver|Silver,Silver|Gold", "Skill_Number_Arr": "3|6|3|6, 5|3, 10|6, 8|15, 3|9, 12|1", "Skill_QP_Arr": "20000|40000|120000|160000|400000|500000|1000000|1200000|2000000", "Break_Name_Arr": "银色暗杀棋子|银色暗杀棋子,凶骨|金色暗杀棋子,蛮神的心脏,虚影之尘|金色暗杀棋子,蛮神的心脏,人造人幼体", "Break_Img_Arr": "figure7s.png|figure7s.png,bone.png|figure7g.png,heart.png,powder.png|figure7g.png,heart.png,potion.png", "Break_Border_Arr": "Silver|Silver,Copper|Gold,Gold,Copper|Gold,Gold,Silver", "Break_Number_Arr": "3|6,11|3,1,10|6,2,6", "Berak_QP_Arr": "15000|45000|150000|450000", "Circle": "12", "TdPointQ": "1.06%", "TdPointA": "1.06%", "TdPointB": "1.06%", "TdPointEx": "1.06%", "whereform": "友情点召唤"}]