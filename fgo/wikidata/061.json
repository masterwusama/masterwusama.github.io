[{"ID": "61", "NAME": "伊丽莎白·巴陶里〔万圣节〕", "NAME_JP": "エリザベート・バートリー〔ハロウィン〕", "NAME_EN": "Erzsebet Bathory （Halloween)", "PIC1": "061A.png", "PIC2": "061B.png", "PIC3": "061C.png", "PIC4": "061D.png", "PIC5": "061Z.png", "Avatar": "061", "STAR": "4", "CLASS": "Caster", "LV1_HP": "1824", "LV1_ATK": "1436", "LVMAX0_HP": "6575", "LVMAX0_ATK": "4997", "LVMAX1_HP": "7351", "LVMAX1_ATK": "5578", "LVMAX2_HP": "8472", "LVMAX2_ATK": "6418", "LVMAX3_HP": "9861", "LVMAX3_ATK": "7460", "LVMAX4_HP": "11404", "LVMAX4_ATK": "8616", "LV90_HP": "12611", "LV90_ATK": "9520", "LV100_HP": "13827", "LV100_ATK": "10432", "COST": "12", "ILLUST": "ワダアルコ", "CV": "大久保瑠美", "GLUTEN": "D", "DURABLE": "D", "AGILE": "D", "MAGIC": "B", "LUCKY": "C", "TREASURE": "D", "SKILL_R1": "无辜的怪物", "SKILL_P1": "605.png", "SKILL_L1": "EX", "SKILL_C1": "7", "SKILL_E1": ["赋予自身每回合获得星星状态（3回合）", "HP回复"], "SKILL_R2": "魔力放出（南瓜）", "SKILL_P2": "306.png", "SKILL_L2": "A", "SKILL_C2": "7", "SKILL_E2": ["自身的Buster卡性能提升（1回合）", "赋予敌方全体灼烧状态（10回合）"], "SKILL_R3": "演出续行", "SKILL_P3": "404.png", "SKILL_C3": "9", "SKILL_L3": "A", "SKILL_E3": ["赋予自身毅力状态（1次·5回合）", "获得星星"], "CSKILL_R1": "阵地作成", "CSKILL_P1": "104.png", "CSKILL_L1": "B", "CSKILL_E1": "自身的Arts卡性能提升（8%)", "CSKILL_R2": "道具作成", "CSKILL_P2": "103.png", "CSKILL_L2": "A", "CSKILL_E2": "自身的弱体成功率提升（10%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "鲜血特上魔娘（Báthory Halloween Erzsébet）", "T_PROP": "Buster", "T_LEVEL": "E-", "T_TYPE": "对人宝具", "T_EFFECT": ["对敌方全体进行无视防御力的强力攻击&赋予诅咒状态（3回合）<超蓄力效果提升>"], "CARD_BUSTER": "1", "CARD_ARTS": "3", "CARD_QUICK": "1", "INTRO1": "身为吸血鬼卡米拉原型的血之伯爵夫人……是因技能“无辜的怪物”而魔人化了的家伙……也是偶然捡到了圣杯而带上了万圣节属性的家伙。已经完全无法理解了啊。\n在原有的倾倒于偶像文化的基础上，又追加了几乎爆棚的爱在城堡里做梦的公主殿下属性的从者。", "INTRO2": "身高/体重：174cm・44kg\n出典：史实\n地域：匈牙利\n属性：混沌・恶 性別：女性\n※身高包含帽子。", "INTRO3": "虽然万圣节原本是凯尔特传说中的收获祭和主题公园之类的东西一点关系都没有，但那种事情和伊丽莎白才是一点关系都没有呢。\n从溢满许多浪漫开始的庆祝，因为是超激动人心的焰火所以要发射出去，相信着一定会收获快乐而制作着南瓜派（烈性药）。", "INTRO4": "龙之女：\n是因为巴陶里家的家纹代表着龙的含义吗，伊丽莎白拥有隔世遗传的龙的因子，由于成为从者而将那因子表面化了。\n虽然看起来像是在万圣节里来临的恶魔，但那角和尾巴都是龙的东西。虎牙并不是吸血鬼的獠牙而是龙的牙齿。", "INTRO5": "“无辜的怪物”是，由于从生前的行为中产生的印象使得过去和存在方式被扭曲，能力、姿态也会发生改变的技能。\n持有同样技能的弗拉德三世，是在其死后，在小说家的创作中被贬为怪物，而她却是从生前的行为开始就遭受这样的待遇了。", "INTRO6": "“鲜血特上魔娘”\n等级：E-\n种类：对人宝具\n（Báthory Halloween Erzsébet）\n召唤出伊丽莎白终生在其中君临天下的居城・监狱城赛伊特，把其当成衬托自己的舞台的宝具。\n虽然某种程度上，在名字上发生了变化，但内容并没有什么特别的变化。", "INTRO7": "把曾说过穿刺拷问、铁处女的故事完全“和我没有关系的话”抛诸脑后，将武器由枪变为了叉的伊丽莎白。\n由于魔力十分富余的原因，尽管并没有特地学习过魔术却可以行使攻击魔术。本人对这个原理是完全不清楚。正像是尽管有许多隐藏才能，却完全不知道其使用方法的伊丽莎白的样子。", "Property": "人型,从者（被EA克制）,龙", "Camp": "人", "Crit": "10.8%", "Death": "42.0%", "CritPr": "49", "InitiativeNp": "1.60%", "Passive": "3.0%", "ArtHit": "1", "BusHit": "1", "QuiHit": "2", "EXHit": "4", "Height": "174", "Weight": "44", "Origin": "史实", "Region": "匈牙利", "Attributes": "混沌・恶", "Gender": "女性", "T_Text_Arr": ["对敌方全体进行无视防御力强力攻击<宝具升级效果提升>", "赋予诅咒状态（3回合）<超蓄力效果提升>"], "T_Num_Arr": [["300%", "400%", "450%", "475%", "500%"], ["500", "1000", "1500", "2000", "2500"]], "S1LV": [["6", "7", "8", "8", "9", "9", "10", "11", "11", "12"], ["1000", "1100", "1200", "1300", "1400", "1500", "1600", "1700", "1800", "2000"]], "S2LV": [["25%", "27%", "29%", "31%", "33%", "35%", "37%", "39%", "41%", "45%"], ["300"]], "S3LV": [["1000", "1150", "1300", "1450", "1600", "1750", "1900", "2050", "2200", "2500"], ["8"]], "Skill_Name_Arr": ["术之辉石", "术之辉石", "术之魔石", "术之魔石,幽灵提灯", "术之秘石,幽灵提灯", "术之秘石,凶骨", "凶骨,龙牙", "龙牙,龙之逆鳞", "传承结晶"], "Skill_Img_Arr": ["sphere04.png", "sphere04.png", "ruby04.png", "ruby04.png,lamp.png", "star04.png,lamp.png", "star04.png,bone.png", "bone.png,tooth.png", "tooth.png,claw1.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Copper", "Copper,Copper", "Copper,Gold", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10, 4", "4, 8", "10, 12", "24, 12", "36, 8", "1"], "Skill_QP_Arr": ["100000", "200000", "600000", "800000", "2000000", "2500000", "5000000", "6000000", "10000000"], "Break_Name_Arr": [["棒棒糖