[{"ID": "103", "NAME": "托马斯·爱迪生", "NAME_JP": "トーマス・エジソン", "NAME_EN": "Thomas Edison", "PIC1": "103A.png", "PIC2": "103B.png", "PIC3": "103C.png", "PIC4": "103D.png", "PIC5": "103Z.png", "Avatar": "103", "STAR": "4", "CLASS": "Caster", "LV1_HP": "1901", "LV1_ATK": "1325", "LVMAX0_HP": "6851", "LVMAX0_ATK": "4611", "LVMAX1_HP": "7660", "LVMAX1_ATK": "5148", "LVMAX2_HP": "8827", "LVMAX2_ATK": "5924", "LVMAX3_HP": "10275", "LVMAX3_ATK": "6885", "LVMAX4_HP": "11882", "LVMAX4_ATK": "7952", "LV90_HP": "13139", "LV90_ATK": "8787", "LV100_HP": "14407", "LV100_ATK": "9628", "COST": "12", "ILLUST": "pako", "CV": "寺岛拓笃", "GLUTEN": "E", "DURABLE": "EX", "AGILE": "E", "MAGIC": "EX", "LUCKY": "A", "TREASURE": "EX", "SKILL_R1": "变化", "SKILL_P1": "400.png", "SKILL_L1": "C", "SKILL_C1": "7", "SKILL_E1": ["自身的防御力提升（3回合）"], "SKILL_R2": "大量生产", "SKILL_P2": "605.png", "SKILL_L2": "A", "SKILL_C2": "12", "SKILL_E2": ["赋予自身每回合获得星星状态（5回合）", "每回合NP获得状态（5回合）"], "SKILL_R3": "概念改良", "SKILL_P3": "313.png", "SKILL_C3": "8", "SKILL_L3": "A+", "SKILL_E3": ["己方单体使用宝具时的蓄力段阶提升2段阶（1回合）", "星星发生率提升（1回合）"], "CSKILL_R1": "阵地作成", "CSKILL_P1": "104.png", "CSKILL_L1": "EX（相当于D)", "CSKILL_E1": "自身的Arts卡性能少许提升（4%)", "CSKILL_R2": "道具作成", "CSKILL_P2": "103.png", "CSKILL_L2": "EX（相当于D)", "CSKILL_E2": "自身的弱体成功率少许提升（4%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "W・F・D（World Faith Domination）", "T_PROP": "Arts", "T_LEVEL": "EX", "T_TYPE": "对民宝具", "T_EFFECT": ["对敌方全体进行强力攻击&赋予宝具封印状态（1回合）&赋予技能封印状态（1回合）&暴击发生率降低（3回合）<超蓄力效果提升>"], "CARD_BUSTER": "1", "CARD_ARTS": "3", "CARD_QUICK": "1", "INTRO1": "托马斯・阿尔瓦・爱迪生是生于美国的发明家。\n作为生于19世纪的人类有着世界顶尖的知名度，不过由于是新晋者，作为Servant的力量略显劣势。\n这次的召唤中为了弥补这一点，将支撑美国这个国家的历代总统处理为“爱迪生”，进行了概念补正。", "INTRO2": "身高/体重：182cm・88kg\n出典：史实\n地域：北美\n属性：秩序・中庸\u3000\u3000性别：男性\n因为和生前肉体的组成相异，身高和体重均与史实记载不同。", "INTRO3": "爱迪生的能力（幸运除外）呈现出不是E等级就是EX的极端状态，\n这是因为他处于特殊召唤的状态，其实质性的能力处于D~E等级。\n\n职阶技能也是相当于“D”的EX等级。\n因为是新晋英灵，得到了补强，能力才会显示出诸多混乱吧。", "INTRO4": "没必要在这里歌颂发明王爱迪生的业绩。\n比起进行新的发明，他更擅长将先人的发明进行再次构造，变成更适合普及的形态……在这一点上他是极其优秀的。\n\n当代的诸多日常制品，都是基于爱迪生的发明。", "INTRO5": "他有一个终生的对手、宿敌、一见面就会打起来的人，那便是Servant尼古拉・特斯拉。\n面对贝尔或巴贝奇时，他会保持理性的去与他们交流。\n但不知为何，只有面对特斯拉的时候是例外的。", "INTRO6": "『W・F・D』\n等级：EX  种类：对民宝具\n范围：不明  最大捕捉：不明\nWorld Faith Domination。\n依靠爱迪生三大发明进行幻想支配。\n发明了照亮黑暗（电灯）、记录流逝的声音（留声机）、映射现实（摄影机与放映机）的装置的爱迪生，使世界的隐秘变得不复存在。\n正确来说，是“存在过，但却被爱迪生夺走了”。\n通过隐藏才能发挥力量的东西就这么被随手揭露，无法计量的力量被固定为零，将民众对神秘的信仰贬低为零，使出“抢夺世界信仰”的——对民宝具。", "INTRO7": "即使变成了狮子的头，智力也完全没有变化。\n拥有简单易懂的性格，生前的熟人，只要见到他稍微说两句话就能明白“啊，这是爱迪生”。\n\n和作为Servant的他相处，几乎不需要注意什么。\n不过他一天的睡眠只有三到四小时，除此以外的时间会全部奉献给劳动……\n生前标准被无意识地强加，所以将他作为Servant使用的时候，要注意睡眠不足和过度劳累。", "Property": "人型,从者（被EA克制）", "Camp": "人", "Crit": "10.6%", "Death": "60.0%", "CritPr": "51", "InitiativeNp": "0.51%", "Passive": "3.0%", "ArtHit": "3", "BusHit": "3", "QuiHit": "3", "EXHit": "5", "Height": "182cm", "Weight": "88kg", "Origin": "史实", "Region": "北美", "Attributes": "秩序・中庸", "Gender": "男性", "T_Text_Arr": ["（宝具本开放后）对敌方全体进行强力的攻击<宝具升级效果提升>", "赋予敌方全体技能封印状态（1回合）", "宝具封印状态（1回合）", "敌方全体暴击发生率下降（3回合）<超蓄力效果提升>", "攻击力下降（3回合）<超蓄力效果提升>"], "T_Num_Arr": [["600%", "750%", "825%", "862.5%", "900%"], ["∅"], ["∅"], ["10%", "20%", "30%", "40%", "50%"], ["10%", "20%", "30%", "40%", "50%"]], "S1LV": [["16%", "16.8%", "17.6%", "18.4%", "19.2%", "20%", "20.8%", "21.6%", "22.4%", "24%"]], "S2LV": [["5", "6", "6", "7", "7", "8", "8", "9", "9", "10"], ["5%", "5.5%", "6%", "6.5%", "7%", "7.5%", "8%", "8.5%", "9%", "10%"]], "S3LV": [["∅"], ["10%", "12%", "14%", "16%", "18%", "20%", "22%", "24%", "26%", "30%"]], "Skill_Name_Arr": ["术之辉石", "术之辉石", "术之魔石", "术之魔石,英雄之证", "术之秘石,英雄之证", "术之秘石,禁断之页", "禁断之页,蛮神的心脏", "蛮神的心脏,混沌之爪", "传承结晶"], "Skill_Img_Arr": ["sphere04.png", "sphere04.png", "ruby04.png", "ruby04.png,plate.png", "star04.png,plate.png", "star04.png,paper.png", "paper.png,heart.png", "heart.png,claw.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Copper", "Gold,Copper", "Gold,Silver", "Silver,Go