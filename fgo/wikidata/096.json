[{"ID": "96", "NAME": "岩窟王 爱德蒙·唐泰斯", "NAME_JP": "巌窟王 エドモン・ダンテス", "NAME_EN": "Edmond Dantes", "PIC1": "096A.png", "PIC2": "096B.png", "PIC3": "096C.png", "PIC4": "096D.png", "PIC5": "096Z.png", "Avatar": "096", "STAR": "5", "CLASS": "Avenger", "LV1_HP": "1785", "LV1_ATK": "1953", "LVMAX0_HP": "7978", "LVMAX0_ATK": "8323", "LVMAX1_HP": "9682", "LVMAX1_ATK": "10075", "LVMAX2_HP": "11044", "LVMAX2_ATK": "11476", "LVMAX3_HP": "11906", "LVMAX3_ATK": "12363", "LVMAX4_HP": "12177", "LVMAX4_ATK": "12641", "LV90_HP": "12177", "LV90_ATK": "12641", "LV100_HP": "13340", "LV100_ATK": "13838", "COST": "16", "ILLUST": "小松崎類", "CV": "岛崎信长", "GLUTEN": "B", "DURABLE": "A+", "AGILE": "C", "MAGIC": "B", "LUCKY": "─", "TREASURE": "A", "SKILL_R1": "钢铁的决意", "SKILL_P1": "300.png", "SKILL_L1": "EX", "SKILL_C1": "8", "SKILL_E1": ["赋予自身无敌贯通状态（1回合）", "攻击力提升（1回合）", "弱体耐性提升（3回合）"], "SKILL_R2": "黄金律", "SKILL_P2": "602.png", "SKILL_L2": "A", "SKILL_C2": "8", "SKILL_E2": ["自身的NP获得量提升（3回合）"], "SKILL_R3": "困境的智慧", "SKILL_P3": "513.png", "SKILL_C3": "8", "SKILL_L3": "A", "SKILL_E3": ["敌方单体的蓄气值减少1格", "解除自身的弱体状态", "获得星星"], "CSKILL_R1": "复仇者", "CSKILL_P1": "107.png", "CSKILL_L1": "A", "CSKILL_E1": "自身受到伤害时获得NP量提升（20%)&自身以外的己方全体（含候补)弱体耐性降低【副作用】（10%)", "CSKILL_R2": "忘却补正", "CSKILL_P2": "108.png", "CSKILL_L2": "B", "CSKILL_E2": "自身的暴击威力提升（8%)", "CSKILL_R3": "自我回复（魔力)", "CSKILL_P3": "109.png", "CSKILL_L3": "D", "CSKILL_E3": "赋予自身每回合获得NP状态（3%)", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "虎呀，燃烧着煌煌火光（Enfer Château d'If）", "T_PROP": "Quick", "T_LEVEL": "A", "T_TYPE": "对军宝具", "T_EFFECT": ["对敌方全体进行强力攻击&防御力降低（3回合）&赋予诅咒状态（5回合）"], "CARD_BUSTER": "2", "CARD_ARTS": "1", "CARD_QUICK": "2", "INTRO1": "知名度最高的复仇者。\n通称“岩窟王”或者“基督山伯爵”。\n因为恶毒的阴谋而被判冤罪，进入了地狱般的伊夫堡（Château d"If）监狱。\n不过由于钢铁般的意志力，他并未因此而绝望。\n最终他获得了基督山岛的财宝并重新回到了巴黎，\n并将那些君临法兰西的部分实力派分子，也就是当初陷害了自己的人送入了地狱。", "INTRO2": "身高/体重：185cm・75kg\n出典：大仲马著《基督山伯爵》？\n地域：法国\n属性：混沌・恶\u3000\u3000\u3000性别：男性\n口头禅是“──等待，并怀抱希望吧”", "INTRO3": "他那残酷的生存方式，隐姓埋名的复仇剧，\n对被人强行夺走蹂躏了的恋人梅尔塞苔丝的思恋和执着，\n以及因苦恼和后悔而痛改前非的经历，让法国乃至全世界的人们讴歌他为“世界最有名的复仇者”。\n\n岩窟王的故事虽然是亚历山大・仲马创作的，不过引导被关进伊夫堡的爱德蒙的“法利亚神甫”，在现代被确认为实际存在的人物……", "INTRO4": "虽然真名是爱德蒙・唐泰斯，不过他也认识到了自己和马赛的海之男“爱德蒙・唐泰斯”并不是同一人物。\n因为“爱德蒙・唐泰斯”在巴黎上演惨绝的复仇剧后舍弃了自身的恶性……\n而作为从者现界的自己则作为“复仇鬼的偶像”存在着。\n他自我评价道：“如果是这样的话，自己并不是爱德蒙”。", "INTRO5": "『岩窟王』\n等级：C  种类：对人宝具\n范围：─  最大捕捉：1人\nMonte Cristo Mythology。\n他是复仇的化身，因此和任何职阶都与他不相称。\n最终，他以特殊职阶・复仇者而现界，他的生存方式也升华成了宝具。\n此宝具可以让他用强韧的肉体与魔力进行攻击\n还可以隐藏自己的数据和职阶，让人看到伪造的情报。\n\n这是永久发动型的宝具。虽说还具备真名解放后的效果，不过本作中并未使用。", "INTRO6": "『虎啊，燃烧着煌煌火光』\n等级：A  种类：对人/对军宝具\n范围：1-20  最大捕捉：1-100人\nEnfer Château d"If。\n\n是在有如地狱一般的伊夫堡培养出的钢铁般的精神力化成的宝具。\n不光是肉体上的牢狱，就连时间、空间这样的无形之牢狱也可以轻松逃脱。\n通过超高速的思考，并强行将其反映到肉体之上，主观上来讲他可以使用类似“时间停止”的超高速行动。\n\n在本作中，宝具呈现出了高速移动产生的“分身”可同时进行复数次攻击的形式。", "INTRO7": "我名为岩窟王（Monte Cristo）。\n不知爱与情为何物，心中只有因憎恶和复仇而熊熊燃烧的怨念之黑炎，直到一切归于灰烬为止会一直带来灾厄，这样的Avenger非我莫属。\n在这个世上没有宠姬（Ede），那此身将以永劫之复仇鬼的身份长存——", "Property": "人型,从者（被EA克制）", "Camp": "人", "Crit": "5.9%", "Death": "7.0%", "CritPr": "30", "InitiativeNp": "0.62%", "Passive": "6.0%", "ArtHit": "2", "BusHit": "3", "QuiHit": "4", "EXHit": "5", "Height": "185", "Weight": "75", "Origin": "大仲马著《基督山伯爵》？", "Region": "法国", "Attributes": "混沌・恶", "Gender": "男性", "T_Text_Arr": ["对敌方全体进行强力的攻击<宝具升级效果提升>", "敌方全体防御力下降（3回合）<超蓄力效果提升>", "赋予敌方全体诅咒状态（5回合）<超蓄力效果提升>"], "T_Num_Arr": [["600%", "800%", "900%", "950%", "1000%"], ["20%", "25%", "30%", "35%", "40%"], ["500", "750", "1000", "1250", "1500"]], "S1LV": [["∅"], ["30%", "32%", "34%", "36%", "38%", "40%", "42%", "44%", "46%", "50%"], ["14%", "15.8%", "17.6%", "19.4%", "21.2%", "23%", "24.8%", "26.6%", "28.4%", "32%"]], "S2LV": [["20%", "23%", "26%", "29%", "32%", "35%", "38%", "41%", "44%", "50%"], ["1"]], "S3LV": [["∅"], ["∅"], ["10", "11", "12", "13", "14", "15", "16", "17", "18", "20"]], "Skill_Name_Arr": ["英雄之证", "世界树之种", "龙牙", "幽灵提灯", "陨蹄铁", "人造人幼体", "混沌之爪", "龙之逆鳞", "传承结晶"], "Skill_Img_Arr": ["plate.png", "walnut.png", "tooth.png", "lamp.png", "clevis.png", "potion.png", "claw.png", "claw1.png", "cubes