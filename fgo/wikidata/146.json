[{"ID": "146", "NAME": "美杜莎〔Lancer〕", "NAME_JP": "メドゥーサ〔ランサー〕", "NAME_EN": "Medusa", "PIC1": "146A.png", "PIC2": "146B.png", "PIC3": "146C.png", "PIC4": "146D.png", "PIC5": "146Z.png", "Avatar": "146", "STAR": "4", "CLASS": "Lancer", "LV1_HP": "2099", "LV1_ATK": "1375", "LVMAX0_HP": "7564", "LVMAX0_ATK": "4786", "LVMAX1_HP": "8457", "LVMAX1_ATK": "5343", "LVMAX2_HP": "9746", "LVMAX2_ATK": "6148", "LVMAX3_HP": "11344", "LVMAX3_ATK": "7145", "LVMAX4_HP": "13119", "LVMAX4_ATK": "8253", "LV90_HP": "14507", "LV90_ATK": "9119", "LV100_HP": "15907", "LV100_ATK": "9993", "COST": "12", "ILLUST": "BLACK", "CV": "浅川悠", "GLUTEN": "C", "DURABLE": "D", "AGILE": "A", "MAGIC": "E", "LUCKY": "C", "TREASURE": "A+", "SKILL_R1": "魅惑的美声", "SKILL_P1": "509.png", "SKILL_L1": "B", "SKILL_C1": "9", "SKILL_E1": ["赋予敌方单体[异性]魅惑状态（1回合）"], "SKILL_R2": "怪力", "SKILL_P2": "300.png", "SKILL_L2": "C", "SKILL_C2": "7", "SKILL_E2": ["自身的攻击力提升（1回合）"], "SKILL_R3": "彼方的追想", "SKILL_P3": "404.png", "SKILL_C3": "9", "SKILL_L3": "A", "SKILL_E3": ["赋予自身毅力状态（1次·3回合）", "赋予自身无敌状态（1次）"], "CSKILL_R1": "对魔力", "CSKILL_P1": "100.png", "CSKILL_L1": "B", "CSKILL_E1": "自身的弱体耐性提升（17.5%)", "CSKILL_R2": "女神的神核", "CSKILL_P2": "327.png", "CSKILL_L2": "A", "CSKILL_E2": "赋予自身伤害提升状态（250)&弱体耐性提升（25%)", "CSKILL_R3": "", "CSKILL_P3": "", "CSKILL_L3": "", "CSKILL_E3": "", "CSKILL_R4": "", "CSKILL_P4": "", "CSKILL_L4": "", "CSKILL_E4": "", "T_NAME": "女神的拥抱（Caress of the Medusa）", "T_PROP": "Quick", "T_LEVEL": "B", "T_TYPE": "对人宝具", "T_EFFECT": "对敌方单体进行超强力攻击&赋予眩晕状态（1回合)<超蓄力效果提升>", "CARD_BUSTER": "1", "CARD_ARTS": "2", "CARD_QUICK": "2", "INTRO1": "希腊神话中登场的戈尔贡三姐妹的三女。\n作为男性憧憬的具象化，以完全体的「偶像（Idol）」之姿\n降生的一尊女神。\n因为某些重要的因素，作为反英雄的美杜莎\n以和作为女神的两位姐姐接近的形式现世。", "INTRO2": "身高/体重：134cm·30kg？\n出典：希腊神话\n地域：欧洲\n属性：中立·善  性别：女性\n关于体重她宣称「和姐姐们一样」。", "INTRO3": "外观上来看是和两位姐姐相似的惹人怜爱的少女，\n性格与作为『Rider』时的自己有所变化。\n但是，与姐姐们『不乞求他人帮助就无法生存下去\n的永恒的少女』的形象所不同，\n业已具备一些战斗能力，因夺取了许多人性命\n恶魔的种子在心中萌芽。", "INTRO4": "『女神的拥抱』\n等级：B   类别：对人宝具\nCaress of the Medusa.\n从前作为『Rider』阶的英灵时所拥有的技能，\n也就是说现在状态下的美杜莎\n以『未来』取得之物作为宝具。 \n自从遭遇她手中所持能斩杀不死之身的刀刃之后，\n能在对方进入自己视野的那一瞬间将其石化，\n这是来自最高等阶的魔眼『库柏勒』的效果。\n她往往以此为核心展开攻势。", "INTRO5": "〇魅惑的美声：B\n和姐姐们拥有同样的固有技能。\n可以对男性施与魅惑的魔术效果，\n但可以被对魔力技能回避。\n即便是没有对魔力\n如果有抵抗的意识的话效果可能因此减轻。", "INTRO6": "〇怪力：C\n短时间内增幅肌肉力量。只有魔物和魔兽才拥有的攻击特性。\n使用之后能使自身的力量提升一个档次。持续时间根据“怪力”等级而不同。\n…因魔力性质比较弱，降低了技能等级。\n\n〇彼岸的遥想：A\n不知在那一天能够抵达的彼岸———\n回想着那份所深爱的岁月，使这思念一直支撑她战斗至最后。", "INTRO7": "作为一位神灵系的从者\n本不应该被召唤。\n尽管感知到人类历史终焉的危机而显现…\n既然在迦勒底应顺召唤而来，\n希望能见到这种姿态的她也\n至少有两个人吧。\n最初虽看上去很难接近，从远处看好像\n除了落泪之外什么都做不到，\n但也会默默地守护着她的。", "Property": "人型,神性,从者（被EA克制）,天/地从者", "Camp": "地", "Crit": "12.2%", "Death": "40.0%", "CritPr": "89", "InitiativeNp": "0.44%", "Passive": "4.0%", "ArtHit": "4", "BusHit": "1", "QuiHit": "3", "EXHit": "5", "Height": "134cm", "Weight": "30kg？", "Origin": "希腊神话", "Region": "欧洲", "Attributes": "中立・善", "Gender": "女性", "T_Text_Arr": "（宝具本开放后）对敌方单体进行超强力的攻击<宝具升级效果提升>-赋予眩晕状态（1回合)<超蓄力效果提升>-Quick卡耐性下降（3回合）", "T_Num_Arr": [["1600%", "2000%", "2200%", "2300%", "2400%"], ["80%", "100%", "120%", "140%", "160%"], ["20%"]], "S1LV": [["60%", "63%", "66%", "69%", "72%", "75%", "78%", "81%", "84%", "90%"]], "S2LV": [["10%", "12%", "14%", "16%", "18%", "20%", "22%", "24%", "26%", "30%"]], "S3LV": [["1000", "1200", "1400", "1600", "1800", "2000", "2200", "2400", "2600", "3000"], ["∅"]], "Skill_Name_Arr": ["槍の輝石", "槍の輝石", "槍の魔石", "槍の魔石,世界樹の種", "槍の秘石,世界樹の種", "槍の秘石,追憶の貝殻", "追憶の貝殻,原初の産毛", "原初の産毛,蛇の宝玉", "伝承結晶"], "Skill_Img_Arr": ["sphere03.png", "sphere03.png", "ruby03.png", "ruby03.png,walnut.png", "star03.png,walnut.png", "star03.png,shell.png", "shell.png,yinmao.png", "yinmao.png,snake.png", "cubes.png"], "Skill_Border_Arr": ["Copper", "Copper", "Silver", "Silver,Silver", "Gold,Silver", "Gold,Silver", "Silver,Gold", "Gold,Silver", "Gold"], "Skill_Number_Arr": ["4", "10", "4", "10,5", "4,10", "10,4", "8,3", "9,16", "1"], "Skill_QP_Arr": ["100000", "200000", "600000", "800000", "2000000", "2500000", "5000000", "6000000", "10000000"], "Break_Name_Arr": [["ランサーピース"], ["ランサーピース